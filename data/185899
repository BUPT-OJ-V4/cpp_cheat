#include<stdio.h>
#include<string.h>
//int buf[1000];
int main()
{
    int t;
    while(scanf("%d",&t)!=EOF)
    {
        while(t--)
        {
        char str[1000];
        scanf("%s",str);
        int len=strlen(str);
        int ans=0;
        bool  flag=false;
        for(int i=0;i<len;i++)
        {
            if(str[i]-'0'==1)
            {
                ans++;
                flag=true;
            }
            else
            {
                flag=false;
                ans=0;
            }

            if(ans==5)
            {
                int j;
                for(j=i+2;j<len;j++)
                    str[j-1]=str[j];
                //str[j]=0;
                len--;
                ans=0;
            }
        }
        for(int i=0;i<len;i++)
        {
            printf("%c",str[i]);
        }
        printf("\n");
        //printf("%s",str);
        //puts(str);
        }
    }
    return 0;
}
