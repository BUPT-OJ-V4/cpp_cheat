#include <stdio.h>
int main(){
int t,n,s,ss;
int f[50];
scanf("%d",&t);
while(t--)
{scanf("%d",&n);
s=0;
for(int j=0;j<n;j++)
{scanf("%d",&f[j]);}
for(int p=0;p<n;p++)
{for(int q=0;q<n;q++)
{for(int k=0;k<n;k++)
	  {ss=f[q]+f[k];
		  if(f[p]==ss)
			  s++;}}}

printf("%d\n",s);}//t
return 0;}