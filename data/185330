#include<stdio.h>


int main()
{
    int n = 0;
    
    char str[1010] = {0};
    
  
    
    while( scanf( "%d", &n ) != EOF )
    {
           for( int i = 0; i < n; i++ )
           {
                
                scanf( "%s", str );
                //printf( "%s\n", str );
                
                
                int NumOf1 = 0;
                putchar(str[0]);
                
                for( int j = 1; str[j]; j++ )
                {
                     if( str[j] == '1' && str[j-1] == '1' ) 
                     {
                         putchar(str[j]);
                         NumOf1++;
                     }
                     else 
                     {
                          putchar(str[j]);
                          NumOf1 = 0;
                     }
                     
                     if( NumOf1 == 4 ) 
                     {
                         //printf( "%d\n", NumOf1 );
                         NumOf1 = 0;
                         j++;
                        // if( str[j] == '\0') break;
                         
                         
                     }
                     
                     
                     
                } 
                printf( "\n");
                
                
           } //for
    }
    

    
    
    
    return 0;
    
    
}
