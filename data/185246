#include<stdio.h>

int main()
{
	int T = 0, N = 0;
	char ch = '0';
	int array[1000], result[1000];
	int ti = 0, ni = 0, ri = 0;
	int count = 0;
	
	scanf("%d", &T);
	scanf("%c", &ch);
	
	for(ti = 0;ti <= T - 1;ti++)
	{
		ch = '0';
		scanf("%c", &ch);
		ni = 0;
		while(ch != '\n')
		{
			if(ch == '0')
				array[ni] = 0;
			else
				array[ni] = 1;
			scanf("%c", &ch);
			ni++;
		}
		N = ni;
		//读取完毕
		for(ni = 0, ri = 0, count = 0;ni <= N - 1;ni++)
		{
			if(array[ni] == 1)
			{
				result[ri] = array[ni];
				ri++;
				count++;
			}
			else
			{
				if(count != 5)
				{
					result[ri] = array[ni];
					ri++;
					count = 0;
				}
				else
				{
					count = 0;
				}
					
			}
		}
		
		for(ni = 0;ni <= ri - 1;ni++)
			printf("%d", result[ni]);
		printf("\n"); 
	}
	
	return 0;
}
