#include<iostream>
#include<cstdio>
#include<stdio.h>
#include<queue>
#include<string.h>
#include<vector>
#include<algorithm>
int uuu ;
using namespace std ;
int n,m ;
int main()
{
    int T ;
    cin>>T ;
    while(T--)
    {
              int i, j;
        cin>>n>>m ;
        int ans = 0 ;
        for(i=1;i<=n;i++)
        {
            int p = m%i ;
            if(p != 0)  continue ;
            int u = m/i ;
            if(u > n)  continue ;
            ans ++ ;
        }
        cout<<ans<<endl;
    }
//    cin>>uuu ;
    return 0 ;
}
