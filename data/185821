#include <iostream>
#include <stdio.h>
#include <string>
using namespace std;

int main()
{
    int n;
    scanf("%d\n", &n);
    while(n--) {
        string s;
        string output = "";
        int count = 0;
        getline(cin, s);
        for(int i=0; i<s.length(); i++) {
            if(count == 5) {
                count = 0;
                continue;
            }
            if(s[i] == '1') {
                count++;
                output += "1";
            }
            if(s[i] == '0') {
                output += "0";
                count = 0;
            }
        }
        cout << output << endl;
    }
    return 0;
}
