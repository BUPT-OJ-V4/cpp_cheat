#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(int argc, char *argv[])
{
  int i, j, t, k, n;
  scanf("%d", &t);
  getchar();
  for(j=0; j<t; j++){
             char s[20000];
             memset(s, '\0', sizeof(s));
             //if()
             gets(s);
             //printf("%d:%s\n",t,s );
             int len = strlen(s) , count=0;
             for(i=0; i<len; ){
                      
                   if(s[i]=='1'){
                        count++;
                        
                        if(count<5){
                                    printf("1");
                                  i++;   
                        }else if(count==5){
                              printf("1");
                              count=0;
                              int m = i+1;
                                     if(m<len){
                                         if(s[m]=='0'){
                                                       i=i+2;
                                         }else{
                                               i++;}
                                                            
                                     }else{
                                           break;}
                              }  
                   }else if(s[i]=='0'){
                                 printf("0");
                                 count=0;
                                 i++;
                   }
                         
                    
             }
             printf("\n");
  }
	
  return 0;
}
