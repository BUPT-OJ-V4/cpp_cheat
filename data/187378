#include<stdio.h>
#include<string.h>
char a[1010],ans[10010];
int Size;
int main()
{

    int T,i;
    scanf("%d",&T);
    while(T--)
    {
        scanf("%s",a);
        int len=strlen(a);
        Size=0;
        int cnt=0;
        for(i=0;i<len;i++)
        {
            if(cnt<5)
            {
                ans[Size++]=a[i];
                if(a[i]=='1')
                {
                    cnt++;
                }
                else
                    cnt=0;
            }
            else
            {
                cnt=0;
            }

        }
        for(i=0;i<Size;i++)
        {
            printf("%c",ans[i]);
        }
        printf("\n");
    }

    return 0;
}