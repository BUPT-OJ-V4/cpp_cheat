#include <stdio.h>
//#include <stdlib.h>
int main(){
    int t,i,count;
    char c[1001];
    scanf("%d",&t);
    while(t--){
               
       for(i=0;i<1000;i++){
          c[i]='\0';
       }
       scanf("%s",&c);
       
       count=0;
       for(i=0;c[i]!='\0';i++){
          if(count!=5){
             if(c[i]=='0'){
                count=0;
                printf("%c",c[i]);
             }
             else if(c[i]=='1'){
                count++;
                printf("%c",c[i]);
             }
          }
          else{
             count=0;
          }
       }
       printf("\n");
    }
    //system("pause");
    return 0;
}
    
