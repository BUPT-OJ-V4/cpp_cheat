#include<cstring>
#include<cstdio>
#include<cmath>
#include<algorithm>
#include<cstdlib>
using namespace std;
char p[1005];
int main()
{
    int i,j,k,n,sum,t,m;
    while(scanf("%d",&t) != EOF){
        while(t --){
            scanf("%d%d",&n,&m);
            sum = 0;
            for(i = 1;i <= n;i ++){
                if(m % i == 0 && m / i <= n){
                    sum ++;
                }
            }
            printf("%d\n",sum);
        }
    }
    return 0;
}
