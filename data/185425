#include <stdio.h>

int main()
{
    int i,T;
    char a[1001];
    scanf("%d",&T);
    while(T--)
    {
              int sum=0;
             scanf("%s",&a); 
             
             int len=0;
             for(i=0;a[i]!='\0';i++)
              len++;
             
             for(i=0;i<len;i++)
              {
                    printf("%c",a[i]);
                     if(a[i]=='0')
                     {
                                  sum=0; 
					
					continue;
                }
				
				sum++;
				if(sum==5) {
					sum=0;
					i++;

				}
}
                                    printf("\n");
	}
    //system("PAUSE");
    return 0;
}
