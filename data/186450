#include<stdio.h>
#include<stdlib.h>
#include<string.h>


int main()
{
    char a[1001];
    char b[1001];
    int n;
    int cnt;
    int p;
    int len;
    int i,j,k,l;
    scanf("%d",&n);
    for(i = 0; i < n; i++)
    {
        cnt = 0;
        scanf("%s", a);
        len = strlen(a);
        if(len<=4)
        {
            for(p = 0; p <= len; p++)
                b[p] = a[p];
            for(l = 0; l < p; l++)
                printf("%c", b[l]);
            printf("\n");
        }
        else if(len >=5)
        {
           for(p = 0; p <= 4; p++)
                b[p] = a[p];
            for(j = 5; j < len; j++)
            {
                if(a[j] == '0'&&a[j-1] == '1' && a[j-2] == '1' && a[j-3] == '1' &&a[j-4] == '1' &&a[j-5] == '1')
                {
                    continue;
                }
                else
                {
                    b[p++] = a[j];
                }
            }
            for(l = 0; l < p; l++)
                printf("%c", b[l]);
            printf("\n");
        }
    }
}
