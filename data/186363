#include"stdio.h"
void main(){
    int T=0,b=0,c=0,i=0,j=0,k=0,num=0,tmp=0;
    int *a;
    scanf("%d",&T);
    a=(int *)malloc(T*sizeof(int));
    for(i=0;i<T;i++){
        scanf("%d",&b);
        scanf("%d",&c);
        for(j=1;j<=b;j++){
            k=c%j;
            tmp=c/j;
            if(k==0&&tmp<=b){
                num++;
            }
        }
        a[i]=num;
        num=0;
    }
    for(i=0;i<T;i++){
        printf("%d\n",a[i]);
    }
}
