#include<stdio.h>
int main(){
    int t;
    //int mx[][];
    scanf("%d",&t);
    int n,m;
    int a,b;
    int count;
    for(a=0;a<t;a++){
                     scanf("%d%d",&n,&m);
                     count = 0;
                     for(b=1;b<=n;b++){
                                       if((double)m <= ((double)b)*((double)n))
                                       if(m%b == 0)
                                       count++;
                                       }
                     printf("%d\n",count);
                     }
    return 0;
}
                     
