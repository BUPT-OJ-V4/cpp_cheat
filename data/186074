#include<stdio.h>
#include<stdlib.h>

int main(){
    int t;
    scanf("%d",&t);
    while(t--){
               int n,m;
               scanf("%d %d",&n,&m);
               int count=0;
               for(int i=1;i<=n;i++){
                       int c=m%i;
                       int d=m/i;
                       if(c==0&&d<=n)
                                     count++;
                       
                       }
               printf("%d\n",count);
               }
    return 0;
    }
