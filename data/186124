#include<stdio.h>
#include<stdlib.h>
#include<iostream>
#include<algorithm>
#include<string.h>
using namespace std;
int main()
{
    int t,i;
    char a[1001],b[1001];
    cin>>t;
    while(t--){
               cin>>a;
               int len1=strlen(a);
               int num=0;
               for(i=0;i<len1;i++){
                                   if(a[i]=='1'){
                                        num+=1;
                                        cout<<a[i];
                                        if(num==5){
                                                   if(i!=len1-1&&a[i+1]=='0'){i++;num=0;}
                                                   }
                                        }
                                   else{
                                       cout<<a[i];
                                       num=0;}
                                   }
               cout<<endl;
               }
    
    return 0;
}