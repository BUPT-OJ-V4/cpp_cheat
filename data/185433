#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void){
    int num = 0;
    scanf("%d", &num);
    for(int i = 0; i < num; i++){
            char str[1001];
	    scanf("%s", str);
            int len = strlen(str);
            for(int j = 0; j < len; j++){
                    if(str[j] == '0'){
                    	continue;
                    }
                    else if(str[j] == '1'){
                         int count = 0;
                         for(int k = 0; k < 5; k++){
                                 if(str[j+k] == '1'){
                                 	count++;
                                 }
                         }
                         if(count == 5){
                         	str[j+5] = '2';
                         	j = j+5;
                         }
                    }
            }
            for(int j = 0; j < len; j++){
                    if(str[j] == '2');
                    else
	                 printf("%c", str[j]);
            }
            printf("\n");
    }
    return 0;
}