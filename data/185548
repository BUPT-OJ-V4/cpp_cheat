#include<iostream>
#include<string>
#include<cstring>
using namespace std;
int main()
{
    int t,len,count,i,j;
    string a;
    int c[1005];
    while(cin>>t)
    {
                 while(t--)
                 {
                           cin>>a;
                           int len=a.length();
                           count=0;
                           for(i=0;i<len;i++)
                           {
                                             if(a[i]=='0')
                                             {
                                             c[i]=0;
                                             count=0;
                                             }
                                             else if(a[i]=='1')
                                             c[i]=++count;
                           }
                           for(i=0;i<len;i++)
                           {
                                             if(c[i]==5 && i!=len-1)
                                             {
                                                        cout<<a[i];
                                                        i++;
                                             }
                                             else 
                                             cout<<a[i];
                           }
                           cout<<endl;
                 }
    }                                                                  
    //system("pause");                        
    return 0;
}