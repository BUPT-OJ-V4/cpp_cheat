#include <iostream>
using namespace std;

int main()
{
    int t=0;
    cin>>t;

    while(t--)
    {
        string str;
        cin>>str;
        int k=0;
        for(int i=0; i<str.size(); i++)
        {
            if( i+1<str.size() &&  i+2<str.size() && i+3<str.size() && i+4<str.size()
                    && str[i]=='1' && str[i+1]=='1' && str[i+2]=='1' && str[i+3]=='1' && str[i+4]=='1')
            {
                str.erase(i+5,1);
                i=i+4;
            }
        }

        cout<<str<<endl;
    }

    return 0;
}
