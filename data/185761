#include<stdio.h>
#include<math.h>


int main()
{
    int T = 0;
    
   
    while(scanf( "%d", &T) != EOF )
    {
                 
                 
              for( int i = 0; i < T ;i++ )
              {
                   
                   int n = 0, m = 0;
                   
                   int num = 0;
                   
                   scanf( "%d%d", &n, &m );
                   
                  
                   
                   for( int j = 1; j <= n; j++ )
                   {
                        if( m % j  == 0 && m / j <= n ) num++;
                        
                        
                   }//input
                   printf( "%d\n", num );
                   
              }   
            
                 
                
                 
    }
    
   
  
    return 0;
    
    
}
