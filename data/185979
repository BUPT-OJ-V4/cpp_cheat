#include<stdio.h>
int main()
{
    int t;
    while(scanf("%d",&t)!=EOF){
        while(t--){
            int n,m;
            scanf("%d%d",&n,&m);
            int count=0;
            int s;
            for(int i=1;i<=n;i++){
                if(m<=i*n){
                    s=i;
                    break;
                }
            }
            for(int i=s;i<=n;i++){
                if(m%i==0)
                    count++;
            }
            printf("%d\n",count);
        }
    }
    return 0;
}
