#include <stdio.h>
#include <stdlib.h>
#include <vector>
#include <string>
#include <string.h>
#include <iostream>
#define maxn 10005
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
         string s;
         cin>>s;
         int flag=0;
         int count=0;
         string a;
         string b;
          for(unsigned int i=0;i<s.length();i++)
          {   
                         if(count==5)
                         {
                              a=a.append(s,i-5,5);
                              flag=1;
                              count=0;
                              b.erase(0,b.length());
                          } 
                          else
                          {
                              if(s[i]=='1')
                              {
                                 b=b.append(s,i,1);
                                 flag=1;
                                 count++;
                                 if(i==s.length()-1) 
                                 {
                                       a=a.append(b,0,b.length());              
                                 }                      
                             }   
                             else
                             {
                                 if(flag)
                                 {
                                     a=a.append(b,0,b.length());
                                     b.erase(0,b.length());
                                 }
                                 flag=0;
                                 count=0;
                                 a=a.append(s,i,1);
                             }  
                        } 
                  
          }
           for(unsigned int i=0;i<a.length();i++)
           {
               cout<<a[i];
            }     
           cout<<endl;
    } 
   return 0; 
} 
