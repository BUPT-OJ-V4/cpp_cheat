#include <cstdlib>
#include <cstdio>
#include <string>
#include <algorithm>
#include <iostream>
using namespace std;

int main(){
    int n, m;
    cin >> m;
    getchar();
    while(m--){
            string str;
            getline(cin, str);
            int count = 0;
            int t = 0;
            if(str.substr(0, 8) == "01111110") t += 8; 
            for(string::iterator it = str.begin() + t; it != str.end() - t; it++){
                    if(*it == '1'){
                          count++;
                    }else{
                          count = 0;
                    }
                    if(count == 5 && it + 1 != str.end() - t){
                                   str.erase(it + 1); 
                                   count = 0;        
                    } 
            } 
            cout << str << endl;                            
    }
    return 0;
}
