//#include<iostream>
#include<stdio.h>
int main(){
    int a[101];
    int n,t,i,j,sum,z;
    scanf("%d",&t);
    while(t--){
          scanf("%d",&n);
          for(i = 0; i < n; i++){
                scanf("%d",&a[i]);
                }         
          sum = 0;
          for(i = 0; i < n; i++){
                for(j = 0; j < n; j++){
                      for(z = 0; z < n; z++){
                              if(a[i] + a[j] == a[z]){
                                      sum++;
                              } 
                      }
                }
          }
          printf("%d\n",sum);
          
               
    }
            
    //system("pause");
    return 0;
    }
