#include<stdlib.h>
#include<stdio.h>
#include<string.h>
int A[101];
void Delete(char *s,int x)
{
    int pos=x+5;
    int len=strlen(s);
    for(int i=pos;i<len;i++)
    {
        s[i]=s[i+1];
    }
}
int main()
{
int n,i,j;
while(scanf("%d",&n)!=EOF)
{
      while(n--)
      {

          char s[1001];
