          char pattern[7]="111110";
          int l1=strlen(s);
          int l2=strlen(pattern);
          int ans[1000];
          int size=0;
for(i=0;i<=l1-l2;i++)
{
                    int flag=0;
    int temp=i;
    for(j=0;j<l2;j++,temp++){
        if(s[temp]!=pattern[j])
           {flag=1;
           }
            
    }
    if(flag==0)
         
         {Delete(s,i);  
         i+=4;
         }
}
for(int i=0;i<size;i++)
   {
     
   }
   printf("%s\n",s);
      }


