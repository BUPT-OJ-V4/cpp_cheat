#include<iostream>
#include<algorithm>
#include<string>
#include<math.h>
using namespace std;
int main(){
    int i,j,k,count=0;
    int num,n,n2,m,l,mi;
    cin>>num;
    for(k=0;k<num;k++){
                     cin>>n>>m;
                     n2=sqrt(m);
                     for(i=1;i<=n2;i++){
                                       if(m%i == 0 && (m/i)<=n && i*i!=m )
                                       {
                                       count=count+2;
                                       }
                                       else if(m%i == 0 && (m/i)<=n && i*i==m ){
                                            count =count+1;
                                            }
                        }
                     cout <<count<<endl;
                     count = 0;    
                     
                     }
                     
                     return 0;
}
