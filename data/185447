#include <stdio.h>
#include <string.h>

int main(){
	int T,i;
	void test();
	scanf("%d",&T);
	for(i=0;i<T;i++){
		test();
	}
	return 0;
}

void test(){
	char a[1000];
	int len,i,j,count=0;
	scanf("%s",a);
	len=strlen(a);
	for(i=0;i<len;i++){
		if(a[i]=='0'){
			count=0;
			continue;
		}else{
			count++;
			if(count==5){
				a[i+1]='x';
				i++;
				count=0;
			}
			continue;
		}
	}
	for(i=0;i<len;i++){
		if(a[i]!='x'){
			printf("%c",a[i]);
		}
	}
	printf("\n");
}