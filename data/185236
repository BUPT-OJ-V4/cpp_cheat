#include<iostream>
#include<string>
using namespace std;
int main()
{
	int t,k,i,num;
	string s;
	cin>>t;
	for(k=0;k<t;k++)
	{
		num=0;
		cin>>s;
		for(i=0;i<s.size();i++)
		{
			if(s[i]=='1')
			{
				num++;
			}
			else{
				num=0;
			}
			if(num==5)
			{
				s.erase(i+1,1);
				num=0;
			}
		}
		cout<<s<<endl;
	}
	return 0;
}