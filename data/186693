#include <stdio.h>
#include <algorithm>
#include <string.h>
using namespace std;
int main()
{
    int n, m, i, j;
    int T, flag;
    char s[1003];
    
    scanf("%d", &T);
    
    while(T-- > 0)
    {
              flag = 0;
              scanf("%s", s);
              n = strlen(s);
              i = 0;
              
              while(i < n)
              {
                    if(s[i] == '1')
                    {
                            flag++;
                    }
                    else flag = 0;
                    printf("%c", s[i]);
                    
                    if(flag == 5)
                            {
                                 
                                 i++;
                                 flag = 0;
                                 
                                 }
                    i++;

              }
              printf("\n");
                        
              
    }
    
    //getchar();
    return 0;    
}
