#include<stdio.h>
#include<string.h>
int main(){
	int T;
	scanf("%d",&T);
	for(int i=0;i<T;i++){
		char a[1001];
		int b[1001]={0};
		scanf("%s",a);
		int count=0;
		int len = strlen(a);
		for(int i=0;i<len-1;i++){
			if(a[i]=='1'){
				count++;
			}
			if(count!=5&&a[i]!='1'){
				count=0;
			}if(count==5&&a[i+1]=='0'){
				count=0;
				b[i]=1;
			}
			}
		for(int i=0;i<len;i++){
			printf("%c",a[i]);
			if(b[i]==1){
				i++;
			}
			if(i==len-1){
		      printf("\n");
			}
	}
	}
	
			return 0;
	}
