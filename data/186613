#include<cstdio>
#include<iostream>
using namespace std;

int main(){
    int n;
    scanf("%d",&n);
    for(int i= 0;i != n;i++){
        int dimen,target,cnt = 0,temp;
        scanf("%d%d",&dimen,&target);
        for(int j = 1;j <= dimen;j++){
            temp = target/j;
            if(target%j == 0 && temp <= dimen){
                cnt += 1;
            }
        }
        printf("%d\n",cnt);
    }
}
