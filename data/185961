//#include <cstdlib>
#include <iostream>
#include<stdio.h>
#include<string>
//#include<iomanip>
//#include<algorithm>
using namespace std;
int main()
{
    int n;
    for(;cin>>n;)
    {        
               
     for(int i=0;i<n;i++)
     {   string s;  
         cin>>s;   
             
        int l=s.length(); 
        int t=0;  
        int sum=0;            
       
        for(int j=0;j<l-t;j++)
          {
                if(s[j]=='0')
                {
                     s[j]=s[j];
                }
                else if(s[j]=='1')  
                {
                      sum++;
                     if(s[j]==s[j]+1)
                   {
                        sum++;             
                   }
                   else if(s[j]!=s[j+1])
                   {
                        if(sum==5)
                    {                                
                        for(int k=j+1;k<l;k++)
                        {
                                s[k]=s[k+1];
                        }   
                            t++;                     
                    }
                        sum=0;
                   }
                    
                }
          }    
                 
          for(int k=0;k<l-t-1;k++)
          {
                  cout<<s[k];
          }
          cout<<s[l-t-1]<<endl;
               }
         }
   
   
    return 0;
}
