#include <stdio.h>
#include <string.h>
int main()
{
    int t, p;
    scanf("%d", &t);
    getchar();
    for(p=1; p<=t; p++)
    {
        char s[2000];
        int i, j;
        //fflush(stdin);
        gets(s);
        int sum = 0;
        for(i=0; i<strlen(s); i++)
        {
            if(s[i] == '1')
            {
                sum++;
                printf("%c", s[i]);
            }
            else if(s[i] == '0')
            {
                if(sum == 5)
                {
                    sum = 0; continue;
                }
                else
                {
                    printf("%c", s[i]);
                    sum = 0;
                }
            }
        }
        printf("\n");
    }
    return 0;

}
