#include<cstdio>
#include<iostream>
#include<cstring>
#include<algorithm>
using namespace std;
int main(){
    int T;
    while(scanf("%d",&T)!=EOF){
    while(T--){
       char str[1010];
       scanf("%s",str);
       int len=strlen(str);
       int n=len;
       int count=0;
       for(int i=0;i<len;i++){
               
               if(str[i]=='1'){
               count++;
               }
              
               if(count==5){
                  if(str[i+1]=='0'&&(str[i+2]=='0'||str[i+2]=='1')){
        
                     for(int j=i+1;j<len-1;j++){
                        str[j]=str[j+1];
                     }
                     n=n-1;
                     //i=i+1;
                     count=0; 
                     continue;
                  }
               }
               
               if(str[i]=='0'){
               count=0;
               }
               
       }
       for(int i=0;i<n;i++){
               printf("%c",str[i]);
       }
       printf("\n");
    }
}
}
  
