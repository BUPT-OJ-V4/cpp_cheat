#include<stdio.h>
#include<stdlib.h>
int num[51];
int main(){
    int t;
    while(scanf("%d",&t)!=EOF){
       while(t--!=0){
           int n;
           int ans=0;
           scanf("%d",&n);
           for(int i=0;i<n;i++)
           scanf("%d",&num[i]);
           
           for(int i=0;i<n;i++){
             int x=num[i];
             for(int j=0;j<n;j++){
             for(int k=0;k<n;k++){
             if(num[j]+num[k]==x)
                ans++;
                               
                }
                            
                }
                    
                    
                }         
                printf("%d\n",ans);  
                }   
                                       
                               
                }
    //system("pause");
    return 0;
    }
