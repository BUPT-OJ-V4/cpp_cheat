#include <stdio.h>
#include <stdlib.h>

int main()
{
    int T;
    scanf("%d", &T);
    while(T--)
    {
        int n, m;
        scanf("%d%d", &n, &m);
        int count = 0;
        int i;
        for(i = 1; i <= n && m >= i; ++i)
        {
            if (m % i == 0 && m / i <= n) ++count;
        }
        printf("%d\n", count);
    }
    return 0;
}
