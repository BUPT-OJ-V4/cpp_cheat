#include<stdio.h>
#include<string.h>
using namespace std;
char a[1010];
int main()
{   
    int T;
    while(scanf("%d",&T)!=EOF){
       while(T--){
         scanf("%s",a);
         int len=strlen(a);
         int sum=0;
         for(int i=0;i<len;i++){
             if(a[i]=='1'&&a[i+1]=='1'&&a[i+2]=='1'&&a[i+3]=='1'&&a[i+4]=='1'&&a[i+5]=='0'){
                    for(int j=i+5;j<len-1;j++){
                    a[j]=a[j+1]; 
                 }
                 sum++;
                 i=i+4;
         }
         a[len-sum]=0;
         }
         for(int i=0;i<len-sum;i++){
                 printf("%c",a[i]);
         }
         printf("\n");
         }
         break;
         }
    return 0;
}