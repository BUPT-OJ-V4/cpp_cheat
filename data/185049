#include<iostream>
#include<cmath>
#include<cstring>
#include<algorithm>
#define maxn 1005
using namespace std;
char str[maxn];

int main()
{
    int tes,len,i;
    while(cin>>tes)
    {
        while(tes--)
        {
            cin>>str;
            len=strlen(str);

            int cnt=0;
            for(i=0;i<len;i++)
            {
                if(str[i]=='0')
                {
                    cout<<str[i];
                    cnt=0;
                }
                else
                {
                    cout<<str[i];
                    cnt++;
                    if(cnt==5)
                    {
                        i++;
                        cnt=0;
                    }
                }
            }
            cout<<endl;
        }
    }
    return 0;
}
