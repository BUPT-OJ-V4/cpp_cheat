#include <iostream>
#include <cstdio>
#include <cstring>
using namespace std;
char a[1010];
int main(int argc, char *argv[])
{
    int T;
    while(scanf("%d",&T)!=EOF){
    //scanf("%d",&T);
         while(T--){
           cin>>a;
           int l=strlen(a);
           int ans=0;
           int flag=0;
           for(int i=0;i<l;i++){
                   if(a[i]=='1'){ans++;flag=i;}
                   else {ans=0;}
                   if(ans==5){
                      for(int j=flag+1;j<l-1;j++){
                              a[j]=a[j+1];
                      }
                      ans=0;
                      l--;
                   }
                   
           }
           for(int i=0;i<l;i++){
                   cout<<a[i];
                   }
                   printf("\n");
         }
    }
    return 0;
}
