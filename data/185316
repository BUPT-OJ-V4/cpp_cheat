#include<stdio.h>
int main()
{
    int T=0;
    int i=0,count=0;
    char Bits[1001] = {0};
    scanf("%d",&T);
    while(T--)
    {
        scanf("%s",Bits);
        for(i=0;Bits[i]!=0;i++)
        {
            if(Bits[i]=='1')
            {
                 printf("1");
                 count++;
                 if(count==5)
                 {
                     count=0;
                     i++;
                 }
            }
            else if(Bits[i]=='0')
            {
                 printf("0");
                 count = 0;
            }
        }
        printf("\n");             
    }
    return 0;
}
