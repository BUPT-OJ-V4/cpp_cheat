#include <iostream>
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <algorithm>
#include <stdlib.h>

using namespace std;


int main()
{
    int T;
    cin>>T;
    while(T--){
            int n,m,c,y;
            cin>>n>>m;
            int num=0;
            for( int i=0;i<n;i++){
                    c=m/(i+1);
                    y=m%(i+1);
                if(y==0&&c>=1&&c<=n)num++;
            }
            cout<<num<<endl;



    }
    return 0;
}
