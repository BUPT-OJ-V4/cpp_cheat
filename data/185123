#include <iostream>
#include <fstream>
#include <cstring>
using namespace std;
int main(void)
{
    //freopen("1.txt", "r", stdin);
    int n;
    cin>>n;
    while(n--)
    {
        int m;
        cin>>m;
        int num[110];
        for(int i = 0; i < m; i++)
            cin>>num[i];
        int cnt = 0;
        for(int i = 0; i < m; i++)
            for(int j = 0; j <m; j++)
                for(int k = 0; k < m; k++)
                    if(num[i] + num[j] == num[k])
        {
            cnt++;
        }
        cout<<cnt<<endl;

    }
    return 0;
}
