#include<cstdio>
#include<vector>
#include<cstdlib>
using namespace std;

int main(){
    int n;
    char bit;
    scanf("%d",&n);
    scanf("%c",&bit);
    for(int i =0;i != n;i++){
        int cnt=0;
        vector<char> vec;
        vector<char>::iterator it;
        scanf("%c",&bit);
        while(bit != '\n'){
            if(bit == '1'){
                cnt+=1;
                vec.push_back(bit);
            }
            else{
                if(cnt != 5){
                    vec.push_back(bit);
                }
                cnt = 0;
            }
            scanf("%c",&bit);
        }
        for(it = vec.begin();it != vec.end();it++)
            printf("%c",*it);
        printf("\n");
    }
}
