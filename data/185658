#include <stdio.h>
#include <iostream>
using namespace std;
int main()
{
	int t;
	scanf("%d",&t);
	getchar();
	while(t--)
	{
		int a = 0;
		char ch;
		ch = getchar();
		int flag = false;
		while(ch!='\n')
		{
			if(ch=='1')
			{
				a++;
				if(a!=5)
				{	
					cout<<ch;	
				}
				else
				{
					cout<<ch;
					flag = true;
					a=0;
				}
			}
			else
			{
				a = 0;
				if(!flag)
					cout<<ch;
				else
					flag = false;
			}
			ch = getchar();
		}
		cout<<endl;
	}
}