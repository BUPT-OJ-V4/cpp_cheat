#include<stdio.h>
int main()
{
int T,n,a[51];
scanf("%d",&T);
int m,i,j,k,c;
for(m = 1;m<=T;m++)
{
    c = 0;
    scanf("%d",&n);
    //printf("%d\n",n);
    for(i = 1;i<=n;i++)
    scanf("%d",&a[i]);
for(i = 1;i<=n;i++)
        for(j = 1;j <= n;j++)
           for(k = 1;k <= n;k++)
           if(a[i]+a[j] == a[k])
            c++;
            printf("%d",c);
            printf("\n");

}
return 0;
}