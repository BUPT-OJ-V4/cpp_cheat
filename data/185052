#include<stdio.h>
int main(){
    int t;
    scanf("%d",&t);
    for(int x=0;x<t;x++){
            int n,a[51],count=0;
            scanf("%d",&n);
            for(int i=0;i<n;i++){
                    scanf("%d",&a[i]);
                    }
                    for(int i=0;i<n;i++){
                            for(int j=0;j<n;j++){
                                    for(int k=0;k<n;k++){
                                            if(a[i]==a[j]+a[k]) count++;
                                            }
                                    }
                            }
                 printf("%d\n",count);
            }
return 0;
}
