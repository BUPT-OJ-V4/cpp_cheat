#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{   int sum;
    scanf("%d",&sum);
    for(int i=0;i<sum;i++)
    {
        char instr[1001]={'\0'};
        char outstr[1001]={'\0'};
        scanf("%s",instr);
        int num=0;
        int index=0;
        for(int j=0;j<strlen(instr);j++)
        {
            if(instr[j]=='1')
            {
                outstr[index]='1';
                index++;
                num++;
            }
            else
            {
                if(num==5)
                {
                    num=0;
                }
                else
                {
                    outstr[index]='0';
                    index++;
                    num=0;
                }
                
            }
            
        } 
        printf("%s\n",outstr);  
        
    }
    
    //system("pause");
    return 0;
}
