#include<stdio.h>
#include<string>
#include<iostream>
#include<vector>
#include<map>
#include<string.h>
#include<cmath>
#include<algorithm>
using namespace std;
char seq[1001];
int main()
{
    int t;
    scanf("%d",&t);
    getchar();
    while(t--)
    {
        gets(seq);
        int len = strlen(seq);
        int count = 0;
        for(int i=0;i<len;i++)
        {
            if(seq[i]=='1')
            {
                count++;
                printf("1");
            }
            else if(seq[i]=='0')
            {
                if(count!=5)
                {
                    printf("0");
                }
                count=0;
            }
        }
        printf("\n");
    }
    return 0;
}
