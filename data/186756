#include <iostream>
#include<string.h>

using namespace std;

int main()
{
    int T;
    cin>>T;
    for(int i=0;i<T;i++)
    {
            int times=0;
            char a[1000]={'0'};
            int len=strlen(a);
            cin>>a;
            for(int j=0;j<1000;j++)
            {
                    if(a[j]<0)
                    {
                              break;
                    }
                    else
                    {
                        len++;
                    }
            }
            for(int j=0;j<len;j++)
            {
                    if(a[j]=='0')
                    {
                                 times=0;
                    }
                    if(a[j]=='1')
                    {
                                 times++;
                    }
                    if(times==5)
                    {
                                for(int k=j+1;k<len-1;k++)
                                {
                                        a[k]=a[k+1];
                               
                                }
                                times=0;
                               
                             
                    }
                 
            }
            
            len=strlen(a);
            for(int j=0;j<len;j++)
            {
                    cout<<a[j];
            }
            cout<<endl;
           
     
    }
   
    return 0;
}
