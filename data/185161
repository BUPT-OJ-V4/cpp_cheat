#include<iostream>
#include<cstdio>
#include<string>
using namespace std;
int main()
{
    int T,flag;
    scanf("%d",&T);
    while(T--){
        string a,b;
        flag=0;
        cin >> a;
        for(int i=0;i<a.length();i++){
            if(flag==5){
                flag=0;
                continue;
            }
            if(a[i]=='1'){flag++;}
            else {flag=0;}
            b+=a[i];
        }
        cout <<b<< endl;
    }
    return 0;
}
