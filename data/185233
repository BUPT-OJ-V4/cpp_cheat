#include <iostream>
#include <stdlib.h>
#include <stdio.h>
#include <algorithm>
#include <map>
#include <string>
#include <vector>
using namespace std;
int main(){
    int T;
    cin>>T;
    while(T--){
               string temp;
               cin>>temp;
               int count = 0;
               for(int i = 0; i < temp.size(); i++){
                       while(temp[i] == '1'){
                                     count++;
                                     i++;
                       }
                       if(count == 5){
                                temp[i] = '#';
                       }
                       count = 0;
               }
               for(int i = 0; i < temp.size(); i++){
                       if(temp[i] == '#')
                                  continue;
                       else
                           cout<<temp[i];
               }
               cout<<endl;
    }  
}
