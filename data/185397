#include <iostream>
#include <fstream>
#include <cstring>
#include <string>
#include <vector>
#include <map>
#include <cstdlib>
#include <algorithm>
#include <cmath>
using namespace std;
int T;
string func(string& input){
    int count=0,pos =0;
    string out;
    while(pos != input.size()){
        if(pos == 0){
            count = (input[pos] == '0'?0:1);
            out += count+'0';
            pos++;
            continue;
        }
        if(count ==0 && input[pos] == '0'){
            out += '0';
            count = 0;
        }
        else if(count ==0 && input[pos] == '1'){
            count = 1;
            out+='1';
        }
        else if(count !=0 && input[pos] == '0'){
            if(count == 5){
                count = 0;
            } else{
                out+='0';
                count = 0;
            }
        }
        else if(count !=0 && input[pos] == '1'){
            count++;
            out+='1';
        }
        else{}
        //cout<<out<<" "<<input[pos]<<" "<<count<<endl;
        pos++;
    }
    return out;
}
int main()
{
    //fstream in("sample.txt");
    cin>>T;
    string input,out;
    for(int time=0;time!=T;++time){
        cin>>input;
        out = func(input);
        cout<<out<<endl;
    }
    return 0;
}
