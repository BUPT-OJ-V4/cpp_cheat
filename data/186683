#include <stdio.h>
int main()
{
    int T;
    int k=0;
    long long int i,j,n,m;

    int flag;

    scanf("%d",&T);
    while(k<T)
    {
        scanf("%lld %lld",&n,&m);
        flag=0;

        for(i=1;i<=n;i++)
        {
          if(m%i==0&&m<=i*n)
            flag++;
        }
        printf("%d\n",flag);



        k++;
    }

    return 0;
}
