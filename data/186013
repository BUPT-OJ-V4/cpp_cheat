#include<stdio.h>

int main()
{
    int T;
    scanf( "%d", &T );
    while( T-- )
    {
           int n;
           long long m;
           scanf( "%d %lld", &n, &m );
           int count=0;
           for( int i = 1; i <= n; i++ )
           {
                for( int j = m / i; j <= n; j++ )
                {
                     if( i * j == m )
                     count++;
                     if( i*j >m )
                     break;
                }
                if( i>m )
                break;
            }
            printf( "%d\n", count );
    }
    return 0;
}
