#include <stdio.h>
#include <cstdlib>
#include <string.h>

int main()
{
    int N;
    while(scanf("%d",&N) != EOF)
    {
        for(int ii = 0; ii < N ;ii++)
        {
                char str[1001];
                scanf("%s",str);
                int len = strlen(str);
                char strAn[1001];
                
                int flag = 0;
                int kk = 0;
                for(int jj = 0;jj <len;jj++)
                {
                        if(str[jj] == '1')
                        {
                         flag ++;           
                         }
                         else
                         {
                            flag = 0;
                         }
                         
                          strAn[kk] =str[jj];
                             kk++;
                         if(flag == 5)
                         {
                                 jj++;
                             flag = 0;  
                                
                         }
                                                                                                          
                       
                            
                         
                            
                        
                }       
                
                strAn[kk] = '\0'; 
                 printf("%s\n",strAn);       
        }                 
        
    }                
    
    
    
   // system("pause");
    return 0;
    
}
