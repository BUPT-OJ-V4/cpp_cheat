#include<stdio.h>
int a[55];
int main()
{   
    int T;
    while(scanf("%d",&T)!=EOF){
        while(T--){
          int n;
          int count=0;
          scanf("%d",&n);
          for(int i=1;i<=n;i++)
          scanf("%d",&a[i]);
          for(int i=1;i<=n;i++){
                  for(int j=1;j<=n;j++){
                          for(int l=1;l<=n;l++){
                                  if(a[i]+a[j]==a[l])
                                  count++;
                   }
             }
          } 
          printf("%d\n",count);
          }
          break; 
          } 
    return 0;
}
