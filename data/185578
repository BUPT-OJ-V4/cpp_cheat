#include <stdio.h>
#include <stdlib.h>

int main()
{
  int t,i,flag,nums,len;
  int a[1100];
  char str[1100];
  while(scanf("%d",&t)!=EOF)
  {
    while(t--)
    {
        scanf("%s",str);
        len = strlen(str);
        for(i = 0; i < len; i++) a[i] = str[i] - '0';
        flag = 0; nums = 0;
        for(i = 0; i < len; i++)
        {
            if(a[i] == 1 && flag == 1 && nums < 4)
            {
                printf("%d",a[i]);
                flag = 1;
                nums++;  
                continue;  
            }    
            else if(a[i] == 1 && flag == 1 && nums == 4)
            {
                printf("%d",a[i]);
                i++;
                flag = 1;
                nums = 0;
                continue;  
            }
            else if(a[i] == 1 && flag == 0 )
            {
                printf("%d",a[i]);
                flag = 1;
                nums = 1;
                continue;  
            }
            else if(a[i] == 0 && flag == 0)
            {
                printf("%d",a[i]);
                nums = 0;
                flag = 0; 
                continue;    
            }
            else if(a[i] == 0 && flag == 1)
            {
                printf("%d",a[i]);
                nums = 0;
                flag = 0; 
                continue;    
            }
        }
        printf("\n");
        
    }///one zu      
  }
  
  //system("PAUSE");	
  return 0;
}