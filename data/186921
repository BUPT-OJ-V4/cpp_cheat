#include<iostream>
#include<cmath>
#include<cstring>
#include<algorithm>
#define maxn 1005
using namespace std;

char s[maxn];
char t[maxn];
char tmp[maxn];
char op[15];

int main()
{
    int n,i;
    while(cin>>s)
    {
        strcpy(t,"");
        cin>>n;

        int l,r,p;
        int len1,len2;
        while(n--)
        {
            cin>>op;
            if(strcmp(op,"CUT")==0)
            {
                cin>>l>>r;
                len1=strlen(s);
                len2=r-l+1;

                for(i=0;i<len2;i++)
                {
                    t[i]=s[i+l];
                }
                t[i]='\0';

                for(i=0;i<l;i++)
                    tmp[i]=s[i];
                for(i=l;i<len1-len2;i++)
                    tmp[i]=s[i+r-l+1];
                tmp[i]='\0';

                strcpy(s,tmp);
                cout<<s<<endl;
            }
            else if(strcmp(op,"COPY")==0)   //ok
            {
                cin>>l>>r;
                len2=r-l+1;
                for(i=0;i<len2;i++)
                    t[i]=s[l+i];
                t[len2]='\0';

                cout<<s<<endl;
            }
            else
            {
                cin>>p;

                //cout<<s<<endl;
                //cout<<t<<endl;
                for(i=0;i<=p;i++)
                {
                    tmp[i]=s[i];
                }

                len2=strlen(t);
                len1=strlen(s);
                for(i=p+1;i<=p+len2;i++)
                {
                    tmp[i]=t[i-p-1];
                }
                for(i=p+len2+1;i<len1+len2;i++)
                {
                    tmp[i]=s[i-len2];
                }
                tmp[i]='\0';

                strcpy(s,tmp);

                cout<<s<<endl;
            }
        }
    }
    return 0;
}
