#include<stdio.h>
#include<stdlib.h>
#include<iostream>
#include<string.h>
#include<string>
using namespace std;
int T;
int n;
char s[1001];
int main()
{
    cin >> T;
    while(T--!= 0)
    {
        n = 0;
        cin >> s;
        for(int i = 0;i < strlen(s);i++)
        {
            if((s[i]-'0') == 1)
            {
                n++;
                if(n == 5 && (s[i+1]-'0') == 0)
                {
                    //
                    int j;
                    for(j = i+1;j < (strlen(s)-1);j++)
                    {
                        s[j] = s[j+1];
                    }
                    s[j] = '\0';
                    n = 0;
                }
            }
            else
                n = 0;
        }
        cout << s << endl;
    }

    return 0;
}
