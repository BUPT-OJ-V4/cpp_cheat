#include <stdio.h>
#include <string>
#include <string.h>
#include <iostream>
using namespace std;
char str1[1001];
int main(){
int t,i;
char str2[1001];
scanf("%d",&t);
while(t--){
    scanf("%s",str1);
    int len=strlen(str1);
    for(i=0;i<len;i++){
        str2[i]=str1[i];
    }

    int ans;

    if(str1[0]=='1')
        ans=1;
    else
        ans=0;
    for(i=1;i<len;i++){
         if(str1[i]=='1')
            ans++;
        if(str1[i-1]=='1'&&str1[i]=='0'){
            if(ans==5){
                 str2[i]=' ';
                 ans=0;
            }
            else{
                 ans=0;
                continue;
            }


            }

    }
    for(i=0;i<len;i++){
        if(str2[i]!=' ')
            printf("%c",str2[i]);
    }
    printf("\n");


}
  return 0;
}
