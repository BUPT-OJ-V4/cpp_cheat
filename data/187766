#include<iostream>
#include<math.h>
 
using namespace std;
 
int main()
{
    int T;
    int n,m;
    int ans;
    int i,j,x;
    while(cin>>T)
    {
        for(x=0;x<T;x++)
        {
            cin>>n>>m;
            ans=0;
            for(i=1;i<=n;i++)
            {
                    if(floor(m/i)*i==m&&floor(m/i)<=n)
                    ans++;
            }
            cout<<ans<<endl;
        }
    }
    return 0;
}