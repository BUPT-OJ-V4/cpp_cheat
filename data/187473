#include<stdio.h>
#include<stdlib.h>
//#include<iostream>
#include"math.h"
#include<algorithm>

using namespace std;

int main()
{
    int i,j=0,l,n,oh;
    char k[30][1000];
    //int count[20] = {0};
    while(scanf("%d",&n)!=EOF)
    {
        for(i=0;i<n;i++)
        {
            scanf("%s",&k[i]);
        }
        //if(k[0][1]=='1') printf("fisoasi");
        for(i=0;i<n;i++)
        {
            j=0;
            while(k[i][j]== '0'||k[i][j]=='1')
            {
                if(k[i][j]=='1'&&k[i][j+1]=='1'&&k[i][j+2]=='1'&&k[i][j+3]=='1'&&k[i][j+4]=='1'&&k[i][j+5]=='0')
                {
                    for(l=j+5;k[i][l]=='0'||k[i][l]=='1';l++)
                    {
                        k[i][l]= k[i][l+1];
                    }
                    j+=4;
                }
                j++;
            }
        }
        for(i=0;i<n;i++)
        {
            j=0;
            while(k[i][j]== '0'||k[i][j]=='1')
            {
                oh = k[i][j]-'0';
                printf("%d",oh);
                j++;
            }
            printf("\n");
            }
        }
    return 0;
}
