#include<stdio.h>

int main()
{
   int t;
   scanf("%d",&t);
   
   while(t!=0)
   {
     int n,x=0;;
     scanf("%d",&n);
     int a[n];
     for(int i=0;i<n;i++)
     {
        scanf("%d",&a[i]);     
     }
     for(int j=0;j<n;j++)
     {
        for(int k=0;k<n;k++)
        {
              for(int i=0;i<n;i++)
              {
                  if(a[j]+a[k]==a[i])
                  {
                      x++;
                                     }    
                      }  
                }     
             }
         printf("%d\n",x);                     
     t--;         
}      
     return 0;       
}
    
