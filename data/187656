#include <stdio.h>
int main(){
    
    int n;
    while(scanf("%d",&n)!=EOF){
                          while(n--){
                                    
                            int a;int b;
                            scanf("%d%d",&a,&b);
                             int count=0;
                            for(int i=1;i<=a;i++){
                                   if((b/i<=a)&&b%i==0){
                                        count++; 
                                            
                                              } 
                                    }         
                                     printf("%d\n",count);
                                     }     
                               
                               }
    }
