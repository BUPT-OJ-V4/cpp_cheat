#include<iostream>
#include<stdio.h>
#include<stdlib.h>
#include<algorithm>
#include<math.h>
#include<string.h>
using namespace std;

int main()
{
    int T,i;
    int num;
    char a[1000];
    
    cin>>T;
    if(T<0||T>30)
        return 0;
        
    while(T--)
    {
        scanf("%s",a);               
        num=0; 
        int len=strlen(a);                                          
        for(i=0;i<len;i++)
        {
            if(a[i]=='1')
            {
                num++;
                if((num==5)&&(a[i+1]='0'))
                {
                   a[i+1]='\0';
                   num=0;
                }    
            }
            else
               num=0;
                                       
        }                        
       for(i=0;i<len;i++)
        {
            if(a[i]!='\0')
                cout<<a[i];            
        } 
        cout<<endl;      
                   
    }    
    return 0;        
}
