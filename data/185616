#include<stdio.h>
#include<string.h>
int main()
{
	int t;
	scanf("%d",&t);
	getchar();
	while(t--)
	{
		char str[1000];
		gets(str);
		int len=strlen(str);
		int i,num=0;
		for(i=0;i<len;i++)
		{
			if(str[i]=='1')
				num++;
			else
				num=0;
			if(num==5)
				str[i+1]='2';
		}
		for(i=0;i<len;i++)
		{
			if(str[i]!='2')
				printf("%c",str[i]);
		}
		printf("\n");

	}
}