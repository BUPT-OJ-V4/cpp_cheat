#include<stdio.h>
//#include<stdlib.h>
int main(){
    int t;
    char buf[60][1000];   
    //getchar();
    int a,b;
    int n;
    int cur;
    int count;
    int out;
    char s;
    scanf("%d",&t);
    getchar();
    for(a=0;a<t;a++){
                     //n = 0;
                     //s = '1';
                     //while(s!='\n'){
                     //scanf("%c",&s);
                     //buf[n]= s;
                     //n++;
                     //}
                     //getchar();
                     gets(buf[a]);
                     count = 0;
                     out = 0;
                     //printf("XXX");
                     for(n=0;buf[a][n]!='\0';n++);
                     for(cur=0;cur<n;cur++){
                                            if(count < 5){
                                                     if(buf[a][cur] == '1')
                                                     count++;
                                                     else
                                                     count = 0;
                                                     if(out != 0)
                                                     buf[a][cur-out] = buf[a][cur];
                                                     }
                                            else{
                                                 out++;
                                                 count = 0;
                                                 }
                                            }
                     buf[a][n-out] = '\0';
                     printf("%s\n",buf[a]);
                     }
    //system("pause");
    return 0;
}
                     
                                          
                                                 
                     
