#include <stdio.h>
#include <string>
#include <iostream>
using namespace std;

int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        string str;
        cin>>str;
        int pos=str.find("111110",0);
        while(pos!=string::npos)
        {
            str.erase(pos+5,1);
            pos=str.find("111110",pos+5);
        }
        cout<<str<<endl;
    }
    return 0;
}
