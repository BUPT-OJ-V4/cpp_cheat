#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <algorithm>
#include <math.h>
#include <string.h>
#include <string>

using namespace std;


int main()
{
    int T,n;
    scanf("%d",&T);
    string a="0";
    while(T--){
        cin>>a;
        int lena=a.size();

        int ans=0;

           for (int i=0;i<lena;i++){
            if(a[i]=='0')
                ans=0;
            else if(a[i]=='1'){
                if(++ans == 5&&i<lena-1){
                    a.erase(i+1,1);
                    lena--;
                    ans=0;
                }
            }

        }


        cout<<a<<endl;

    }


    return 0;
}
