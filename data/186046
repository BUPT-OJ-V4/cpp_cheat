#include <cstdlib>
#include <iostream>
#include <stdio.h> 

using namespace std; 

int main()
{
    int t;
    scanf("%d",&t);
    while(t--){
       int n,m,tmp;
       scanf("%d%d",&n,&m);
       int cnt=0;
       for(int i=1;i<=n;i++){
          if(m%i==0){
             tmp=m/i;
             if(tmp<=n) cnt++;
          }
       }
       printf("%d\n",cnt);
    } 
    return 0;
}
