#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i,t,n,m,j,k,d,p,min;
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        scanf("%d %d",&n,&m);
        d=0;
        if(m>=n)
        min=n;
        else  min=m;
        for(p=1;p<=min;p++)
        {
            if((m%p==0)&&((m/p)<=min))
                  d++;
        }
        printf("%d\n",d);
    }
       
       return 0;
    } 
