#include<stdio.h>
#include<string.h>

int main(){
    int NumOfCases;

    scanf("%d",&NumOfCases);
    getchar();
    for(int cases = 0; cases < NumOfCases; cases++){
        char data[1000];
        memset(data,0,1000);


        fgets(data,1000,stdin);

        int length = strlen(data) - 1;

        int counts = 0;
        for(int i = 0; i<length;i++){
            if(data[i] == '1'){
                counts++;
            }else if(data[i] == '0'){
                if(counts == 5){
                        counts = 0;
                        continue;
                }
                counts = 0;
            }
            printf("%c",data[i]);
        }
    printf("\n");
    }
}