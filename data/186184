#include <stdio.h>
#include <stdlib.h>

int main()
{
     int n;
     scanf("%d",&n);
     while(n--){
                char *c=(char*)malloc(sizeof(char));
                scanf("%s",c);
                int i,num,count=0,index=0;
                for(num=0;c[num]!='\0';num++);
                
                for(i=0;i<num;i++){
                                   if(count==5){
                                        
                                         count=0;
                                   }  else{   
                                   if(c[i]=='1');
                                                 count++;
                                   if(c[i]=='0')
                                                count=0;
                                   c[index++]=c[i];
                                   }
                                   
                }
                for(i=0;i<index;i++)
                                    printf("%c",c[i]);
                                    printf("\n");
     }

     return 0;
}
