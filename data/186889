#include<stdio.h>
int main(){
	int T;
	scanf("%d",&T);
	char array[1000];
	while(T>0){
		scanf("%s",array);
		int i=0;
		while(array[i]!='\0'){
			if(array[i]=='1'){
				int j=1;
				for(j=1;j<5;j++){
					if(array[i+j]!='1'){
						break;
					}
				}
				if(j==5&&array[i+j]=='0'){
					int a=i+j;
					while(array[a+1]!='\0'){
						array[a]=array[a+1];
						a++;
					}
					array[a]='\0';
				}
				else if(j==5&&array[i+j]=='1'){
					while(array[i+j]=='1'){
						j++;
					}
					int a=i+j;
					while(array[a+1]!='\0'){
						array[a]=array[a+1];
						a++;
					}
					array[a]='\0';
					
					
				}
				i=i+j;
				i--;
			}
			i++;
			
		}
		printf("%s\n",array);
		T--;
	}
	return 0;
}