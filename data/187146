#include<iostream>
#include<string>
using namespace std;
int main()
{
    int T;
    cin>>T;
    for(int i=0;i<T;i++)
    {
        string str;
        cin>>str;
        string str1;
        int x=0;
        int len=str.length();

        for(int j=0;j<len;j++)
        {
            int y=0;
                if(str[j]=='1')
                {
                    int k;
                    for( k=j;k<j+5&&k<len;k++)
                   {

                    if(str[k]=='0')
                    {   j=k;
                        str1[x++]='0';
                        break;

                    }
                    else if(str[k]=='1')
                    {
                        str1[x++]=str[k];
                        y++;

                    }
                     if(k==len-1)
                        j=len-1;

                   }
                    if(y==5)
                   {
                      j=j+5;
                   }

                }
                else
                    str1[x++]=str[j];

        }

        for(int j=0;j<x;j++)
            cout<<str1[j];
            cout<<endl;
    }
    return 0;
}
