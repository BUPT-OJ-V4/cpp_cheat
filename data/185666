#include<stdio.h>{
int main(){
int i,t,n,j;
scanf("%d",&t);
for(i=0;i<t;i++){
   scanf("%d",&n);
   int num[n];
   for(j=0;j<n;j++){
   scanf("%d",&num[j]);
   }
   int a,b,c;
   int ans=0;
   for(a=0;a<n;a++){
       for(b=0;b<n;b++){
           for(c=0;c<n;c++){
              if(num[a]+num[b]==num[c])ans++;
           }
       }
   }
   printf("%d\n",ans);

}
}