#include <stdio.h>
#include <stdlib.h>

int main()
{
 int n;
    scanf("%d",&n);
    while(n--)
    {
              int num;
              scanf("%d",&num);
              int *a=(int*)malloc(sizeof(int)*num);
              int i,j,k,count;
              count=0;
              for(i=0;i<num;i++)
                scanf("%d",&a[i]);
              for(i=0;i<num;i++){
                for(j=0;j<num;j++){
                  for(k=0;k<num;k++){
                  if(a[i]+a[j]==a[k])
                  count++;
                    
                  
                  
                  
                  }
                                   }
                                     }
                            printf("%d\n",count);           
                            getchar();
                                     
    }
}
