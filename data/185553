#include <iostream>

using namespace std;

int main()
{
    int T;
    cin>>T;
    for(int i=0;i<T;i++)
    {
            int times=0;
            int n;
            cin>>n;
            int *N=new int[n];
            for(int j=0;j<n;j++)
            {
                 cin>>N[j];
            }
            for(int x=0;x<n;x++)
            {
                    for(int y=0;y<n;y++)
                    {
                            for(int z=0;z<n;z++)
                            {
                                    if((N[x]+N[y])==N[z])
                                    {
                                                         times++;
                                        
                                    }
                            }
                    }
            }
            cout<<times<<endl;
            
     
    }
   
    return 0;
}
