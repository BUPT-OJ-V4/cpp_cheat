#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main(){
    int groupNum,n,m,j,k;
    cin>>groupNum;
    
    vector<int> result;
    for(int i=0;i<groupNum;i++){
            cin>>n>>m;
            int count=0;
            if(m==1) count=1;
            if(m>1){
                    for(j=1;j<=n;j++){
                            if((m%j==0)&&(m/j<=n)){
                                 count++;
                            }
                    }        
            }
            result.push_back(count); 
    }
    for(int i=0;i<result.size();i++){
            cout<<result[i]<<endl;
    }

}
