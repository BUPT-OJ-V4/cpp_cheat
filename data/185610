#include <stdio.h>
#include <string.h>

char str[1001];
int tmp[1001];

int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%s",str);
        int len=strlen(str);
        for(int i=0;i<len;i++)
        {
            tmp[i]=str[i]-'0';
        }
        int ans=0;
        for(int i=0;i<len;i++)
        {
            if(tmp[i]==1)
            {
                ans++;
                if(ans==5)
                {
                    for(int j=i+1;j<len-1;j++)
                        tmp[j]=tmp[j+1];
                    len--;
                    ans=0;
                }
            }
            else if(tmp[i]==0)
            {
                ans=0;
            }
        }
        for(int i=0;i<len;i++)
            printf("%d",tmp[i]);\
        printf("\n");
    }
    return 0;
}
