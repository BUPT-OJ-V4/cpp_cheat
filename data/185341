#include <stdio.h>

int main()
{
    int i,j,k,n,T;
    int a[51];
    scanf("%d",&T);
    while(T--)
    {
             scanf("%d",&n);
             for(i=1;i<=n;i++)
             {
                              
                              scanf("%d",&a[i]);
                              }
                              int sum=0;
                              for(i=1;i<=n;i++)
                              for(j=1;j<=n;j++)
                              for(k=1;k<=n;k++)
                              if(a[i]+a[j]==a[k])
                              sum++;
                              printf("%d\n",sum);
	}
    //system("PAUSE");
    return 0;
}
