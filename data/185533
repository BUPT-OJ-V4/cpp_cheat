#include <stdio.h>

using namespace std;

int main() {
    int T;
    while(scanf("%d", &T) != EOF) {
                      for(int i = 0; i < T; i++) {
                              int n,m;
                              int count = 0;
                              scanf("%d %d", &n, &m);
                              for(int j = 1; j <= n; j++) {
                                      if( m %j == 0) {
                                          if(m/j <= n) 
                                                 count++;
                                      }
                              }
                              printf("%d\n", count);
                      }
    }
    
    return 0;
}
