#include<iostream>
using namespace std;
int main()
{
    int a[50];
    int i,j,k,count,t,n;
    while(cin>>t)
    {
                 while(t--)
                 {
                  cin>>n;
                  for(i=0;i<n;i++)
                  {
                                  cin>>a[i];
                  }
                  count=0;
                  for(i=0;i<n;i++)
                  {
                    for(j=0;j<n;j++)
                   {
                      for(k=0;k<n;k++)
                      {
                                      if(a[i]+a[j]==a[k])
                                      count++;
                      }
                   }
                  }
                  cout<<count<<endl;
                  
                  
                 }
    }
    return 0;
}
