import java.util.Scanner;

class Main{
	public static void main(String args[])
	{
		Scanner in = new Scanner(System.in);
		int T = in.nextInt();
		for (int i=0;i<T;i++)
		{
			String t = in.next();
			System.out.println(t.replaceAll("111110", "11111"));
		}
	}
}