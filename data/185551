#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
           char s[1005];
           scanf("%s",s);
           int len=strlen(s);
           int i,cnt=0;
           for(i=0;i<len;i++)
           {
                if(s[i]=='1')
                {
                             cnt++;
                             if(cnt==5)
                             {
                                       for(int j=i;j<len-2;j++)
                                       {
                                            s[j+1]=s[j+2];
                                            }
                                            len--;
                                            cnt=0;
                                       }
                             }
                else
                {
                    cnt=0;
                    }
                
                }
            for(i=0;i<len;i++)
            {
                 if(i!=len-1)printf("%c",s[i]);
                 else printf("%c\n",s[i]);
                 }    
           }
    return 0;
    //system("pause");
    }
