#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <vector>
#include <string.h>
#include <string>
#include <algorithm>
#include <queue>
#include <numeric>

using namespace std;

int main()
{
    // freopen("in1.txt", "r", stdin);

    unsigned int T;
    unsigned int n, m;
    unsigned int res = 0;
    scanf("%u", &T);

    while(T--)
    {
        scanf("%u %u", &n, &m);

        res = 0;
        for(unsigned int i=0; i<n; i++)
        {
            int min_k = i+1;
            int d = i+1;
            int max_k = n*(i+1);

            // printf("%d %d\n", min_k, max_k);

            if(min_k <=m && m<= max_k)
            {
                if(m%min_k == 0)
                    res++;
            }
        }

        printf("%u\n", res);

    }

    // printf("%d\n", T);
    return 0;
}
