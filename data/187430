#include <iostream>
#include <cstring>

using namespace std;

int main(){
    int t, length,num;
    cin>>t;
    char str[1001], temp;
    while(t--){
            num = 0;
        cin>>str;
        length = strlen(str);
        for(int i = 0; i < length; i++){
            if(str[i] == '1'){
                num++;
            }
            else {
                if(num == 5){
                    for(int j = i; j <length; j++){
                        str[j] = str[j+1];
                    }
                    str[length-1] = '\0';
                    length --;

                    i--;
                }
                num = 0;
            }
        }
        for(int i = 0; i <length; i++){
            cout<<str[i];
        }
        cout<<endl;
    }
}
