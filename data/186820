#include<stdio.h>
int main()
{
    int t,i,n,x,y,z;
    while(scanf("%d",&t)!=EOF)
    {
    while(t--)
    {
    
    scanf("%d",&n);
    int a[50];
    for(i=0;i<n;i++)
       scanf("%d",&a[i]);
       int sum=0;
       for(x=0;x<n;x++)
       for(y=0;y<n;y++)
       for(z=0;z<n;z++)
       {
       if(a[x]+a[y]==a[z])
       sum++;
       
              }
              printf("%d\n",sum);
                              }}
                              return 0;
}
