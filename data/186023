#include <iostream>

using namespace std;

int main()
{
    int time=0;
    long long n=0,m=0;
    int mycount=0;
    cin>>time;
    for(int i=0;i<time;i++)
    {
        mycount=0;
        cin>>n>>m;
        for(long long x=1;x<=n;x++)
        {
            if(m%x==0)
            {
                if(m/x<=n)
                {
                    mycount++;
                }
            }
        }
        cout<<mycount<<endl;
    }
    return 0;
}
