#include <stdio.h>
int main(){
   int t;
   while(scanf("%d",&t)!=EOF){
       for(int i=0;i<t;i++){
          char a[1001];
          scanf("%s",a);
          int flag=0;
          for(int j=0;a[j]!='\0';j++){
              if(flag==5){
                 for(int k=j;a[k]!='\0';k++){
                    a[k]=a[k+1];
                 }
                 flag=0;
              }
              if(a[j]=='1')  flag++;
              else flag=0;
          }
          for(int l=0;a[l]!='\0';l++){
              printf("%c",a[l]);
          }
          printf("\n");
       }
   }
   return 0;
}
