#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <algorithm>
using namespace std;

int main(){
    int n;
    while(scanf("%d",&n)!=EOF){
              for(int j=0;j<n;j++){
              
               char str[1010];
                scanf("%s",str);
                int len=strlen(str);
                int count=0;
                for(int i=0;i<len;i++){
                        if(str[i]=='0'){
                                        if(count==5){
                                                     count=0;
                                                     }
                                        else if(count<5){
                                             count=0;
                                              printf("%c",str[i]);
                                             }
                                        
                                        
                                        
                                        }
                        if(str[i]=='1'){
                                        count++;
                                        printf("%c",str[i]);
                                        }
                        }
                printf("\n");
                }
                              }
                             
                              return 0;
}
