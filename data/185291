#include<iostream>
#include<algorithm>
#include<string>
#include<math.h>
using namespace std;
int main(){
    int i,j,count=0,m,n;
    int num;
    string st;
    cin >> num;
    for(j=0;j<num;j++){
        cin>>st;
        for(i=0;i<st.size();i++){
            if(count !=5){
                cout<<st[i];
                if(st[i]=='1'){
                 count++;
                }
                else
                count =0;                
            }
            else if(st[i]=='0'){
                 count = 0;
                 }     
        }
        cout <<endl;
        count = 0;                  
    }

    return 0;
}
