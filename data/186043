#include <iostream>
using namespace std;

int main()
{
    int T,n,m,count;
    cin>>T;
    for(int i=0;i<T;i++)
    {
        count=0;
        cin>>n>>m;
        for(int j=0;j<n;j++)
        {
            if(m%(j+1)!=0)continue;
            for(int k=j;k<n;k++)
            {
                if((j+1)*(k+1)==m)
                {
                    count++;
                    if(j!=k)count++;
                }
                
            }
        }
        cout<<count<<endl;
    }
}
