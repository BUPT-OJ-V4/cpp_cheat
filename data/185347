#include <iostream>
#include <fstream>
#include <cstring>
using namespace std;
int main(void)
{
//    freopen("1.txt", "r", stdin);
//    freopen("2.txt", "w", stdout);
    int n;
    cin>>n;
    while(n--)
    {
        char str[10000];
        cin>>str;
        int cnt = 0;
        for(int i = 0;i < strlen(str);i++)
        {
            if(str[i] == '1')
                cnt++;
            if(str[i] == '0')
                cnt = 0;
            if(cnt == 5)
            {
                cout<<str[i];
                i++;
                cnt = 0;
            }
            else
                cout<<str[i];
        }
        cout<<endl;
    }

    return 0;
}
