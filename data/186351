#include <stdio.h>
#include <stdlib.h>
 
 
int main()
{
    int i,j,k;     
    int num,n,a,count,cur;
    scanf("%d",&num);
 
    for(i=0;i<num;i++){
        count=0;
        scanf("%d",&n);
        scanf("%d",&a);
        if(a<=n){
            for(j=1;j<=a;j++){
                cur=(int)(a/j);
                if(cur*j==a&&cur<=a){
                    count++;  
                }                        
            }
        }
        else{
            for(j=1;j<=n;j++){
                cur=(int)(a/j);
                if(cur*j==a&&cur<=n){
                    count++;  
                }                        
            }
        }
        printf("%d\n",count);                  
    }
     
    //system("pause");   
}
