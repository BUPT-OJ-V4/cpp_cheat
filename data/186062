#include<stdio.h>
#include<stdlib.h>
#include<string.h>
char arr[1001];

int main(){
    int t;
    int len;
    int i,j;
    int m;
    int count;
    scanf("%d",&t);
    while(t--){
               for(i=0;i<1001;i++){
                         arr[i]='\0';
                         }
               scanf("%s",arr);
               len=strlen(arr);
               count=0;
               for(i=0;i<len;i++){
                                  m=arr[i];
                                  if(m==49){
                                                count+=1;
                                                if(count==5){
                                                           for(j=i+1;j<len-1;j++){
                                                                              arr[j]=arr[j+1];
                                                                              }
                                                           arr[len-1]='\0';
                                                           len=strlen(arr);
                                                           count=0;
                                                           }
                                                }
                                  if(m==48){
                                           count=0;
                                           }
                                  }
               printf("%s\n",arr);
               }
    return 0;
    }
