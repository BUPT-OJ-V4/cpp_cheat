#include<stdio.h>
#include<stdlib.h>
int main(){
    int t;
    int i;
    char a[1000];
    int num;
    char temp;
    scanf("%d",&t);
    getchar();
    while(t!=0)
    {
         i=0;
         scanf("%c",&temp);
         while(temp!='\n')
         {
              a[i]=temp;
              i++; 
              scanf("%c",&temp);          
         }
         for(int j=4;j<i;j++)
         {
             if((a[j]=='1')&&(a[j-1]=='1')&&(a[j-2]=='1')&&(a[j-3]=='1')&&(a[j-4]=='1'))
             {
               a[j+1]='a';
             }
         }
         for(int j=0;j<i;j++)
         {
            if(a[j]=='1'||a[j]=='0')
            printf("%c",a[j]);        
         }
         printf("\n");
         t--;     
    }
    //system("pause");
    return 0;
}
