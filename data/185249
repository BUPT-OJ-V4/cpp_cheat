#include <iostream>
#include <string>
#include <vector>
#include <sstream>
#include <algorithm>

using namespace std;

int main() {
    int n;
    cin >> n;
    while (n--) {


    string s;
    cin >> s;
    string result;

    while (true) {
        int begin = s.find("111110");
        if (begin == -1) break;
        result.append(s.substr(0, begin +5));
        s = s.substr(begin + 6);
    }

    result.append(s);

    cout << result << endl;

    }
}
