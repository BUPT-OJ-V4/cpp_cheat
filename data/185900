#include <stdio.h>
#include <stdlib.h>
int main()
{
    //freopen("debug\\in3.txt","r",stdin);
    int t,n,m,x,ans;
    scanf("%d",&t);
    while(t--)
    {
        ans=0;
        scanf("%d %d",&n,&m);
        for(int i=1;i<=n;i++)
        {
            if(m%i==0)
            {
                x=m/i;
                if(i<=n && x<=n)
                    ans++;
            }
        }
        printf("%d\n",ans);
    }
    return 0;
}
