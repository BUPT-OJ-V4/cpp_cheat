#include <iostream>

using namespace std;

int main()
{
    int t,i,flag,count1;
    char data[1000];
    cin>>t;
    while(t--)
    {
        cin>>data;
        count1=0;
        for(i=0;data[i]!='\0';i++)
        {
            if(count1==5)
            {
                if(data[i]=='0')
                {
                    count1=0;
                }
                //else{
                //    cout<<data[i];
                //}
            }
            else
            {
                if(data[i]=='1')
                {
                    count1++;
                    cout<<data[i];
                }
                else
                {
                    count1=0;
                    cout<<data[i];
                }
            }
            //cout<<"["<<count1<<"]";
        }
        cout<<endl;
    }
    return 0;
}
