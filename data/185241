#include <iostream>
#include <cstdlib>
#include <string>

using namespace std;

void ini(int state[1000])
{
     for(int i = 0;i < 1000;i ++)
             state[i] = 0;
}

int main()
{
    int t;
    string num;
    int state[1000];
    
    cin>>t;
    for(int i = 0;i < t;i ++)
    {
        ini(state);
        cin>>num;
        for(int j = 0;j < num.length();j ++)
                state[j] = 1;
        string::iterator it = num.begin();
        int number = 0;
        for(int k = 0;it != num.end();k++,it++)
        {
                if(*it == '1')
                       number++;
                else if(number == 5)
                {
                        state[k] = 0;
                        number = 0;
                }
                else
                    number = 0;
        }
        it = num.begin();
        for(int k = 0;it != num.end();k++,it++)
               if(state[k] == 1)
                    cout<<*it;
        cout<<endl;
    }
    return 0;
}

        
