#include <stdio.h>
#include <stdlib.h>

char s[1001];

int main(int argc, char *argv[])
{
 	int t;
 	scanf("%d",&t);
 	for(int group=0;group<t;group++){
		scanf("%s",s);
		int count=0;
		for(char *c=s;*c!='\0';c++){
	        printf("%c",*c);
	        if(*c=='1'){
			    count ++;
			    if(count ==5){
					c++;
					count=0;
				}
			}
			else{
			    count =0;
			}
		}
		printf("\n");
	}
    //system("PAUSE");
    return 0;
}
