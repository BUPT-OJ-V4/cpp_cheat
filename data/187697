#include <iostream>
#include <stdlib.h>
#include <string>
using namespace std;
int main()
{
    int t;
    while(cin>>t)
    {
          int** da=new int*[t];
          for(int i=0;i<t;i++)
          {
                  string s1;
                  cin>>s1;
                  int count=1;
                  int len=s1.length();
                  for(int j=0;j<len;j++)
                  {
                          
                  
                          if(s1[j]=='1'&&s1[j+1]=='1'&&s1[j+2]=='1'&&s1[j+3]=='1'&&s1[j+4]=='1'&&s1[j+5]=='0')
                          {
         //                            cout<<"f"<<endl;                                                             
                                   if(j+5<len)
                                   {
             //                                 cout<<"ff"<<endl;
                                                for(int k=j+5;k<len-1;k++)
                                                {
                                                        s1[k]=s1[k+1];
                                                }
                                                len--;
             //                                 cout<<s1<<endl;
                                   }
                                   j+=4;
                          }
                  }     
                  da[i]=new int[len+1];
                  da[i][0]=len;
                  for(int j=0;j<len;j++)
                  {
                   da[i][j+1]=s1[j];
        //         cout<<da[i][j+1]-48<<endl;
                  } 
          }
          for(int i=0;i<t;i++)
          {
                  for(int j=1;j<da[i][0]+1;j++)
                  {
                     cout<<da[i][j]-48;
                }
                cout<<endl;
          }   
    }
 //   system("pause");
    return 0;
}
