#include<cstdio>
char array[2000];
int main()
{
    int T=0,t=0;
    scanf("%d",&T);
    getchar();
    while(t++<T)
    {
        char c;
        c = getchar();
        int len=0;
        while(c!='\n'&&c!=-1)
        {
            array[len]=c;
            len++;
            c=getchar();
        }
        int flag = 0;
        for(int i=0;i<len;i++)
        {
            if(flag == 5)
            {
                array[i]='#';
                flag = 0;
                continue;
            }
            if(array[i]=='1')
            {
                flag++;
                continue;
            }
            if(array[i]=='0')
            {
                flag = 0;
                continue;
            }
        }
        
        for (int i=0;i<len;i++)
        {
            if(array[i]!='#')
            {
                printf("%c",array[i]);
            }
        }
        printf("\n");
        
        
    }
    return 0;
}
