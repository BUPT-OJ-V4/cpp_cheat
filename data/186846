#include<stdio.h>

int main()
{
    int T,temp,count=0,flag=0,Q;
    char A[1001];
    for(int i=0;i<1001;i++){A[i]=-1;}
    scanf("%d",&T);
    while(T--)
    {scanf("%s",&A);
    while(A[flag]=='0'||A[flag]=='1')flag++;
    for(int i=0;i<flag;i++)
    {if(A[i]=='1')
       {count++;
       
       if(count==5)
             {temp=i+1;
             count=0;
             
             for(int j=temp;j+1<flag;j++){
               A[j]=A[j+1];}
              flag--; }}
       else count=0;
       
       }
       count=0;
       for(int i=0;i<flag;i++){
       printf("%c",A[i]);}
       printf("\n");
       for(int i=0;i<1001;i++){A[i]=-1;}
       
       
}

return 0;
}