#include<stdio.h>

int main()
{
    int T;
    int i,j,k,m,p,q;
    int count;
    char s[1002];
    int w[1002];
    while(scanf("%d",&T)!=EOF)
    {
        for(i=0;i<T;i++)
        {
            k=0;
            s[k]=getchar();
            while(s[k]=='\n'||s[k]=='\0'||s[k]==' ')
                s[k]=getchar();
            while(s[k]!='\n'&&s[k]!='\0')
            {
                k++;                 //k是长度
                s[k]=getchar();
            }
            count=0;                 //count是计数器
            p=0;
            for(m=0;m<k;m++)
            {
                if(s[m]=='1')
                {
                    count++;
                    if(count==5)
                    {
                        if(s[m+1]=='0')
                        {
                            w[p]=m+1;
                            m++;
                            count=0;
                            p++;
                        }
                        else
                            count--;
                    }
                }
                else
                    if(s[m]=='0')
                    count=0;
            }
            j=0;
            for(q=0;q<p;q++)
            {
                while(j<w[q])
                {
                    printf("%c",s[j]);
                    j++;
                }
                j++;
            }
            while(j<k)
            {
                printf("%c",s[j]);
                j++;
            }
            printf("\n");
        }
    }
    return 0;
}
