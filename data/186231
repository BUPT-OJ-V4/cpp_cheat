#include <stdio.h>
#include <stdlib.h>
#include <string>
#include <string.h>
using namespace std;
int main(){
        int  total;
        while(scanf("%d", &total) != EOF){
              int tmp = total;
              char ans[total + 1][1200];
              while(total != 0){
                int count = 0;
                char nums[1200];
                scanf("%s", nums);
                for(int i = 0; nums[i] != 0; i ++){
                  if(nums[i] == '0'){
                            // printf("%d is zero\n", i);
                    if(count >= 5){
                      nums[i] = 'x';
                      count = 0;         
                    }
                    count =0;           
                  }else if(nums[i] == '1'){
                        //printf("%d is one and count %d\n", i, count);
                    count += 1;        
                  }
                }
                //printf("%s\n", nums);
                  int cnt = 0;
                  for(int i = 0; nums[i] != 0 ; i ++){
                    if(nums[i] != 'x'){
                    ans[total][cnt] = nums[i];
                    //printf("%c", ans[total][cnt]);
                    cnt += 1;
                               }
                  }
                  ans[total][cnt] = 0;
                total -= 1;
              }
              for(int i = tmp; i > 0; i --){
              //printf("tmp:%d\n", tmp);
              for(int j = 0; ans[i][j] != 0; j ++){
                printf("%c", ans[i][j]);
                }
                printf("\n");
              }
        }
        //getchar();
        return 0;
}
