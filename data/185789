#include <iostream>
#include <string.h>
using namespace std;

int main ()
{
    int i, j, n, t, len, count;
    char a[1024];
    while(cin>>t)
    {
        if(t<1 || t>30)
            return 0;
        for(j=0; j<t; ++j)
        {
            cin>>a;
            len = strlen(a);
            if(len>1000)
                return 0;
                
            count = 0;
            for(i=0; i<len; ++i)
            {
                if((count == 5) && (a[i]=='0'))
                {
                    a[i] = '2';
                    count = 0;
                }               
                else if(1 == (a[i]-'0'))
                {
                    ++count;
                }
                else if(a[i]=='0')
                {
                    count = 0;    
                }    
            } 
            for(i=0; i<len; ++i)
            {
                if(a[i]=='2')
                    continue;
                else
                    cout<<a[i];    
            }
            cout<<endl;
        }
        
         
    }
    return 0;
}
