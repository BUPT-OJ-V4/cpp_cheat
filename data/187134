#include<stdio.h>
int main()
{
int t;
char a[1000],b[1000];
scanf("%d",&t);
int k,i,n,j;
for(k = 1;k<=t;k++){
n = 0;
scanf("%s",a);
for(i = 0;a[i]!='\0';i++){
if(a[i] == '0'&&i-5>=0&&a[i-1]=='1'&&a[i-2]=='1'&&a[i-3]=='1'&&a[i-4]=='1'&&a[i-5]=='1'){
    b[n++] = a[++i];
while(a[i]!='\0'){
    if(a[i] == '1');
     break;
     a[n++] = a[i++];
}
}
else b[n++]=a[i];
}
b[n] = '\0';
printf("%s\n",b);
}
return 0;
}

