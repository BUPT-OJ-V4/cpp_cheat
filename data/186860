#include <iostream>
using namespace std;

int main()
{
    int t,n,m,flag,i,shang;
    cin>>t;
    while(t--)
    {
        cin>>n>>m;
        flag=0;
            for(i=1;i<=n;i++)
            {
                if(m%i==0)
                {
                    shang=m/i;
                    //if(shang==i)
                        //flag=1;
                    if((shang>=1)&&(shang<=n))
                        flag++;
                }
            }
            cout<<flag<<endl;
    }
    return 0;
}
