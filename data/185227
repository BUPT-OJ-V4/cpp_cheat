#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n,m;
    int i,j;
    
    char a[1002];
    

    int count;
    
    scanf("%d", &n);
    
    for(i=0;i<n;i++)
    {
                    scanf("%s", a);
                    count = 0;
                    j=0;
                    while(a[j]!='\0')
                    {
                         if(count == 5){
                                  count =0;
                                  j++;
                         }else{
                               
                               if(a[j]=='1')count++;
                               else count=0;
                               printf("%c", a[j]);
                               j++;
                         }           
                    }
                    
                    printf("\n");                    
    }
    
    
    return 0;    
}
