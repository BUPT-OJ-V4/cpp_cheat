#include<stdio.h>
#include<string.h>
int main(){
    int t;
    scanf("%d",&t);
    while(t--){
      char buf[1001];
      scanf("%s",buf);
      int l1=strlen(buf);
      char str[1001];
      int l2=0;
      int count=0;
      for(int i=0;i<l1;i++){
        if(buf[i]-'0'==1){
          count++;
          str[l2++]=buf[i];
        }else{
          if(count==5){
            count=0;
            continue;
          }else{
                str[l2++]=buf[i];
                count=0;
          }
        }
      }
      for(int i=0;i<l1;i++){
        buf[i]=0;
      }
      for(int i=0;i<l2;i++){
              buf[i]=str[i];
      }
      printf("%s\n",buf);
      for(int i=0;i<l2;i++){
        str[i]=0;
      }
    }
    return 0;
}
