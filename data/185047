#include <stdio.h>

int main(){
	int T,i,n;
	void test(int);
	scanf("%d",&T);
	for(i=0;i<T;i++){
		scanf("%d",&n);
		test(n);
	}
	return 0;
}

void test(int n){
	int a[n],i,j,k,count=0;
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			for(k=0;k<n;k++){
				if(a[i]+a[j]==a[k]){
					count++;
				}
			}
		}
	}
	printf("%d\n",count);
}