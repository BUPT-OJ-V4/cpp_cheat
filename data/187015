#include<stdio.h>
#include<math.h>

int main()
{
    int t;
    int n, m;
    int count ;
    int a,b;
    while (scanf("%d",&t) != EOF)
    {
          for (int  i = 0; i < t; i++)
          {
              scanf("%d %d",&n, &m);
              ///判断次数
              count = 0;
              
              double w = sqrt(m);
              
              for (int j = 1; j <= w;j++)
              {
                  a = m/j;
                  b = m%j;
                  
                  if (a < n && j < n && b == 0 && j * j !=  m)
                     count = count +2;
                  else if (j * j == m && j<=n)
                       count = count+1;
              }
              
              //输出次数
              printf("%d\n",count); 
          }
    }
    return 0;
}
