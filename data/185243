#include <cstdlib>
#include <iostream>
#include <string>
using namespace std;
int main()
{
    int T;
    while(cin>>T){
    while(T--)
    {
              string a;
              cin >> a;
              int count = 0;
              int num = a.size();
              for(int i = 0 ; i < num ; i++)
              {
                      if(a[i] == '1')
                      {
                              count++;
                      }
                      else
                      {
                          count = 0;
                      }
                      if(count == 5)
                      {
                               a.erase(i+1,1);
                               count = 0;
                      }
              }
              cout << a <<endl;
    }}
    //system("PAUSE");
    return EXIT_SUCCESS;
}