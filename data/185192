#include<cstdio>
#include<iostream>
#include<cstring>
using namespace std;
int main() {
    //freopen("a.txt", "r", stdin);
    int _;
    scanf("%d", &_);
    while(_--) {
        int n, m;
        scanf("%d%d", &n, &m);
        int ans = 0;
        for(int i = 1; i <= n; ++i) {
                if(i > m) break;
                int d = m - i;
                if(d % i == 0 && (d/i) < n) ++ans;
        }
        printf("%d\n", ans);
    }
    return 0;
}
