#include <stdio.h>
int main(){
    int n;
    while(scanf("%d",&n)!=EOF){
             while(n--){
                        int m;
                        scanf("%d",&m);
                        int ans[101];
                        for(int i=1;i<=m;i++)
                           scanf("%d",&ans[i]);
                           int count=0;
                           int tmp[10001];int size=0;
                           for(int i=1;i<=m;i++){
                              for(int j=1;j<=m;j++){
                                tmp[size++]=ans[i]+ans[j];
                                      }     
                                   
                                   } 
                                   for(int i=0;i<size;i++){
                                       for(int j=0;j<=m;j++){
                                            if(tmp[i]==ans[j])count++;   
                                               }
                                           }    
                                
                                printf("%d\n",count);
                        
                        
                        }                  
                               
                               }
    }
