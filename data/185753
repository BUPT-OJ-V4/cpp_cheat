#include <stdio.h>
#include <stdlib.h>
int main()
{
    //freopen("debug\\in2.txt","r",stdin);
    int t,n,cnt;
    char c;
    scanf("%d",&t);
    getchar();
    while(t--)
    {
        cnt=0;
        while((c=getchar())!='\n')
        {
            if(c=='1')
            {
                if(cnt==4)
                {
                    cnt=0;
                    putchar(c);
                    getchar();
                }else{
                    putchar(c);
                    cnt++;
                }

            }else{
                cnt=0;
                putchar(c);
            }
        }
        putchar(c);
    }
    return 0;
}
