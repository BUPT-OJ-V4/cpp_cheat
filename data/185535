#include <iostream>

using namespace std;

int main()
{
    int time=0;
    char str[1001];
    int mycount;
    cin>>time;
    for(int i=0;i<time;i++)
    {
        mycount = 0;
        cin>>str;
        for(int j=0;str[j]!='\0';j++)
        {
            cout<<str[j];
            if(str[j]=='1')
                mycount++;
            else
                mycount = 0;
            if(mycount==5)
            {
                mycount=0;
                j++;
            }
        }
        cout<<endl;
    }

    return 0;
}
