#include<stdio.h>
#include<stdlib.h>
#include<iostream>
using namespace std;
int main()
{
    int t,n,i;
    int a[51];
    cin>>t;
    while(t--){
               cin>>n;
               for(i=1;i<=n;i++){
                                cin>>a[i];
                                }
               int num=0;
               int j,k;
               for(i=1;i<=n;i++){
                                 for(j=1;j<=n;j++){
                                                   for(k=1;k<=n;k++){
                                                                     if(a[i]+a[j]==a[k])
                                                                     num+=1;
                                                                     }
                                                   }}
               cout<<num<<endl;
               }
    return 0;
}