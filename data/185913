#include<stdio.h>
#include<stdlib.h>
int main(){
    int n;
    scanf("%d",&n);
    getchar();
    for(int i = 0; i <= n-1; i++){
            char c;
            int count = 0;
            scanf("%c",&c);
            while(c != '\n'){
                                   if(c == '1'){
                                          printf("1");
                                          count++;
                                   }
                                   else{
                                        if(count == 5) count =0;
                                        else{
                                             count = 0;
                                             printf("0");
                                        }
                                   }
                                   scanf("%c",&c);
                                   
            }
            printf("\n");
    }
    return 0;
}
