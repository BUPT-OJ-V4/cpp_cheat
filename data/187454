#include<stdio.h>
#include<stdlib.h>
#include<string.h>
using namespace std;
int main(){
    int t,i,j,t1,a;
    char ans[1001];
    char str[1001];
    scanf("%d",&t);
    while(t--){
      scanf("%s",&str);
        t1=strlen(str);
         a=0;
         j=0;
         
         for(i=0;i<t1;){ 
           if(str[i]=='0' && a!=5){
           ans[j++]=str[i++];
           a=0;
          }
            else if(str[i]=='1' && a!=5){
               ans[j++]=str[i++];
               a++;
                }
            else if(a==5){
               i++;
               
               if(str[i]=='1')
                 a=1;
               else if(str[i]=='0')
                 a=0;
                 ans[j++]=str[i++];
               }  
                
         
         }      
         ans[j]='\0';     
        printf("%s",ans);
        printf("\n");       
               
   }
   //system("pause");
    return 0;
    
    }
