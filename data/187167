#include<iostream>
#include<vector>
using namespace std;
int main(){
   int n;
    int s=0;
    std::cin>>s;
    for(n= 0;n< s; n++){
        vector<int>nums;
        int num=0;
        std::cin>>num;
        for(int i=0;i<num;i++){
            int shu=0;
            std:cin>>shu;
            nums.push_back(shu);
        }
    
        int count=0;
        for (int i=0;i<num;i++){
            for (int j=0;j<num;j++){
                int total=nums[i]+nums[j];
                for(int k=0;k<num;k++){
                    if(total==nums[k])
                        count++;
                    
                }
            }
        
}
            std::cout<<count<<endl;
        
}
}