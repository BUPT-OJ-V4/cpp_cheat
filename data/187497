#include<stdio.h>
#include<stdlib.h>
#include<iostream>
#include<string.h>
#include<string>
using namespace std;
int T;
int n;//cout the 1 num
string s;
char t[201];
string w;
char ch;
char temp[201];
int main()
{
    cin >> s;
    cin >> n;
    int a,b;
    for(int i = 0;i <n;i++)
    {
        cin >> w;
        if(w == "CUT")
        {
            scanf("%d %d",&a,&b);
            int k =0;
            for(int j = a;j <= b;j++)
            {
                t[k] = s[j];
                k++;
            }
            t[k] = '\0';
            for(int j = a;j < s.length();j++)
            {
                s[j] = s[++b];
            }
            s[b] = '\0';
            s[s.length()] = '\0';
            cout << s.c_str() << endl;
        }
        else if(w == "COPY")
        {
             scanf("%d %d",&a,&b);
             int k =0;
            for(int j = a;j <= b;j++)
            {
                t[k] = s[j];
                k++;
            }
            t[k] = '\0';
            s[s.length()] = '\0';
            cout << s.c_str() << endl;
        }
        else if(w == "PASTE")
        {
            scanf("%d",&a);
            int len = strlen(t);
            s.insert(a+1,t);
            s[s.length()] = '\0';
            cout << s.c_str() << endl;
        }

    }
    return 0;
}
