#include<cstdio>
#include<iostream>
#include<cstring>
using namespace std;
char s[1111];
bool fg[1111];
int main() {
    //freopen("a.txt", "r", stdin);
    int _;
    scanf("%d", &_);
    while(_--) {
        scanf("%s", s);
        int n = strlen(s);
        for(int i = 0; i < n; ++i) fg[i] = 1;
        for(int i = 0, j = 0; i < n; ++i) {
                if(s[i] == '0') j = i + 1;
                if(i == j+4) {
                     fg[i+1] = 0;
                     j = i + 2;
                }
        }
        for(int i = 0; i < n; ++i) if(fg[i]) printf("%c", s[i]);
        puts("");
    }
    return 0;
}
