#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <vector>
using namespace std;

int main()
{
    int i,j,n,m,t,k,count;
    while(scanf("%d",&t)!=EOF&&t>=1&&t<=20)
    {
        while(t--)
        {
            scanf("%d%d",&n,&m);
            count=0;
            for(i=1;i<=n;i++)
            {
                if(m%i==0)
                {
                    j=m/i;
                    if(j<=i)
                    {
                        if(i==j)
                        {
                            count++;
                        }
                        else
                        {
                            count+=2;
                        }
                    }
                }
            }
            printf("%d\n",count);
        }
    }
    return 0;
}
