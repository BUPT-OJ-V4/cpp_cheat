#include<stdio.h>
#include<stdlib.h>
int main(){
    int count;
    int i=0,j=0;
    char c;
    char test[30][1000];
    int oneNum = 0;
    scanf("%d",&count);
    for(i=0;i<count;i++){
              scanf("%s",test[i]);
    }
    for(i=0;i<count;i++){
          for(j=0;test[i][j]!='\0';j++){
              if(test[i][j]== '1'){
                  oneNum++;
              }
              
              if(test[i][j]== '0'){
                  oneNum = 0;
              }
              printf("%c",test[i][j]);
              if(oneNum == 5 && test[i][j+1] == '0'){
                  j++;
                  oneNum = 0;
              }                            
          } 
          printf("\n");
          
    }
    return 0;
}