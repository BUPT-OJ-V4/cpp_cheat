#include<stdio.h>
#include<string.h>

char buf1[1001];
char buf2[1001];
int main(){
    int t;
    while(scanf("%d",&t)!=EOF){
        int count;
  //      putchar();
        while(t--){
            count=0;
            int sum=0;
            scanf("%s",buf1);
            int len=strlen(buf1);
  //          printf("%d",len);
            for(int i=0;i<len;i++){
                if(buf1[i]=='1'){
                    sum++;
                    buf2[count++]=buf1[i];
                    if(sum==5){
                        sum=0;
                        i++;  
                    }
                }
                else if(buf1[i]=='0') {
                    sum=0;
                    buf2[count++]=buf1[i];
                }
                     
            }
            for(int i=0;i<count;i++)
                 printf("%c",buf2[i]);
            printf("\n"); 
            }
            
            
        }   
       return 0; 
}
