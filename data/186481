#include <stdio.h>
int main()
{
    int T ,num;
    char a[1000],ch;
    int i ,j;
    scanf("%d",&T);
    getchar();
    while(T--)
    {
        i=0;
        while((ch=getchar())!='\n')
        {
            a[i]=ch;
            i++;
        }
        j=i;
        num=0;
        for(i=0; i<j; i++)
        {
            printf("%c",a[i]);
            if(a[i]=='1')

                num++;
            if(a[i]=='0')
                num=0;
            if(num==5)
            {
                i++;
                num=0;
            }
        }
        printf("\n");



    }
    return 0;
}
