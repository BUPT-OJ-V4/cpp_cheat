#include<stdio.h>

int main()
{
    int i,j,T;    //T组  
    int count;  //记录出现多少个一 
    char str[30][1000];  //原序列 
    char str_2[30][1000];  //新序列 
    scanf("%d",&T);
    for(i=0;i<T;i++)  //输入每组数据 
    {
    	scanf("%s",&str[i]);
    }
    
    for(i=0;i<T;i++)
	{
		count=0;
		for(j=0;j<strlen(str[i]);j++)
		{
			if(str[i][j]=='1'&&count!=5)
			{
				count++;
				str_2[i][j]=str[i][j];
			} 
			else if(str[i][j]=='0'&&count!=5)
			{
				str_2[i][j]=str[i][j];
				count=0;
			}
			else if(str[i][j]='0'&&count==5)
			{
				count=0;
				str_2[i][j]=' ';
			}
			if(str_2[i][j]!=' ')
			{
				printf("%c",str_2[i][j]);
			}
		}
		printf("\n");
		
	} 
}