#include <stdio.h>
#include <string.h>

using namespace std;

int main() {
    int T;
    while(scanf("%d", &T) != EOF) {
                      for(int i = 0; i < T; i++) {
                              char ch[1000];
                              int count = 0;
                              int k = 0;
                              scanf("%s", &ch);
                              for(int j = 0; j < strlen(ch); j++) {
                                      if(ch[j] == '1'){
                                               printf("%c", ch[j]);
                                               count++;
                                      }
                                      else {
                                           if(count != 5) {
                                                    printf("%c", ch[j]);
                                           }
                                           count = 0;
                                      }
                              }
                              printf("\n");
                      }
    }
    
    return 0;
}
