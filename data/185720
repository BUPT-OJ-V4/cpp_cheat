#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
    int i=0,p=0,T;
    scanf("%d",&T);
    while(i<T)
    {
        char ch,A[1000]; 
        int b=0;
        memset(A,0,sizeof(A));
        scanf("%c",&ch);
        if(ch=='\n')
           scanf("%c",&ch);
       
        while(ch!='\n')
        {
            A[b]=ch;
            b++;
            scanf("%c",&ch);
            } 
            
        
        A[b]=ch;
        int a,n=0;
        for(a=0;A[a]!='\n';a++)
        {
            if(A[a]=='1')
               n++;
            else if(A[a]=='0')
                    n=0;
            
            if(n<=5)
                printf("%c",A[a]); 
            if(n==5)
            {
                a++;
                n=0;
                }  
            
              
            }
        printf("\n");
        
        i++;
        }
 
    return 0;
    
    }
