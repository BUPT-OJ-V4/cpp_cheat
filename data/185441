#include <stdio.h>
#include <stdlib.h>
char List[1001];
int main(){
    int n,count;
    scanf("%d",&n);
    while(n--){
        scanf("%s",List);
        count=0;
        for(int i=0;List[i]!='\0';i++){
            if(List[i]=='1'){
                count++;
            }else{
                  if(count==5){
                    count=0;
                    continue;
                  }
               count=0;
            }
             

            printf("%c",List[i]);
    
        }
        printf("\n");
    }

   //getchar();getchar();getchar();   
}
