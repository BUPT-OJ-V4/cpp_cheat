#include <iostream>
using namespace std;

int main(){
    int t;
    cin>>t;
    
    char num[t][1000],res[t][1000];
    for(int i=0; i<t; i++)
            cin>>num[i];
    
    int count;
    for(int i=0; i<t; i++){
            count=0;
            int m,n;
            for(m=0,n=0; num[i][m]!='\0'; m++,n++){
                    res[i][n]=num[i][m];
                    if(num[i][m]=='1'){
                            if(++count==5){
                                    m++;
                                    count=0;
                            }
                    }
                    else
                        count=0;
            }
            res[i][n]='\0';
    }
    
    for(int i=0; i<t; i++){
            if(i==t-1)
                    cout<<res[i];
            else
                    cout<<res[i]<<endl;
    }       
    
    return 0;
}
