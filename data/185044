#include <cstdio>
#include <cstring>
#include <iostream>
#include <map>
#include <set>
#include <algorithm>
#include <queue>
#include <stack>
using namespace std;

const int imax_n = 100005;

int a[imax_n];

int n;
char s[imax_n];

int main()
{
    int T;
    scanf("%d",&T);
    while (T--)
    {
        scanf("%s",s);
        int flag=0;
        for (int i=0;s[i];i++)
        {
            if (s[i]=='1')
            {
                printf("1");
                flag++;
            }
            else
            {
                if (flag==5)
                {
                    flag=0;
                    continue;
                }
                printf("0");
                flag=0;
            }
        }
        printf("\n");
    }
    return 0;
}
