#include<iostream>
#include<cstring>
using namespace std;
int main()
{
    char s[1001];
    int count,t,i;
    
    while(cin>>t)
    {
                 while(t--)
                 {
                    cin>>s;
                    count=0;
                    for(i=0;i<strlen(s);i++)
                    {
                            if(s[i]=='1')
                            {     
                            count++; 
                            cout<<s[i]; 
                            }
                            if(s[i]=='0'&&count==5)
                            {
                            count=0;
                            continue;
                            
                            }
                            if(s[i]=='0'&&count!=5)
                            {
                            cout<<s[i];
                            count=0;
                            }
                                       
                    }
                    cout<<endl;
                    
                 }
                 
    }
 return 0;   
}
