#include <cstdlib>
#include <iostream>
#include <cstring>

using namespace std;
char str[1000 + 5];

int main(int argc, char *argv[])
{
    int cases;
    cin >> cases;
    while (cases--)
    {
          cin >> str;
          int l = strlen(str);
          int count = 0;
          int f = 0;
          for (int i = 0; i < l; ++i)
          {
              if (str[i] == '0')
              {
                 if (count == 5 && f == 1)
                 { 
                        //   cout << "dbg 0 : 0";
                     count = 0;
                     f = 0;
                      continue;
                  }
                  else
                      { count = 0; f = 0; cout << str[i]; }
              }else
              {
                  f = 1;
                  ++count;
                  cout << str[i];
              }
          }
          cout << endl;
          }
     
    return EXIT_SUCCESS;
}
