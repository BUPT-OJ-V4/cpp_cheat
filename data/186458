#include<stdio.h>

int main()
{
    char a[1010];
    int count=0;
    char b[1010];
    int t=0;
    int i=0;
    int j=0;
    int x=0;
    
    scanf("%d",&t);
    while(t>0)
    {
              scanf("%s",&a);
              count=i=j=0;
              while(a[i])
              { x=a[i]-'0';
                         if(count<5)
                         {
                                     b[j]=a[i];
                                     j++;
                        
                         if(x==1)
                         {    count++;
     
                         }
                         else
                         { 
                         count=0;
                         }
                         }
                         if(count==5&&x==0)
                         {
                           count=0;
                         }
                         i++;
              }
            
              for(i=0;i<j;i++)
              {
                    printf("%c",b[i]);
              }
              printf("\n");
              t--;
    }
    
  
    return 0;
}
