#include<iostream>
#include<cstdio>
#include<string.h>
using namespace std;
int main(){
    int t;
    while(scanf("%d",&t)!=EOF){
         while(t--){
             char str[1001];
             scanf("%s",str);
             int l=strlen(str);
             int k=0;
             for(int i=0;i<l;i++){
                 if(str[i]=='1'){
                     k++;
                     printf("%c",str[i]);
                 } 
                 else if(str[i]=='0'){
                      if(k==5){
                         k=0;
                         }
                      else {
                           printf("%c",str[i]);
                           k=0;
                           }
                 }
             }
             printf("\n");  
        } 
    }   
    return 0;   
}
