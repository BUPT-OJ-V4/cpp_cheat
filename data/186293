#include<iostream>
#include<cmath>
#include<vector>
using namespace std;

vector<int> vec1;
vector<int> vec2;
int m,p;

int fun1(int n)
{
    int k = (int)sqrt(n-1);
    //cout << k;
    int cnt = 0;
    for (int i = 1; i <= k; i++)
        if (n % i == 0)
        {

            vec1.push_back(i);
            vec2.push_back(n / i);
        }
    int l = vec1.size();
    for (int i = 0; i< l; i++)
        if (vec1[i] <= p && vec2[i] <= p)
            cnt++;
    return 2*cnt;
}

int main()
{
    int T;
    cin >> T;
    while (T--)
    {
        vec1.clear();
        vec2.clear();
        int r = 0;
        cin >> p >> m;
        int t = (int)sqrt(m);
        if (t*t == m && t <= p)
            r = 1;
        cout << r+fun1(m) <<endl;
    }
    return 0;
}