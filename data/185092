#include<cstdio>
#include<iostream>
#include<cstdlib>
#include<cstring>
using namespace std;

int T;
int N;
char lst[1001];

int main()
{
    scanf("%d",&T);
    while(T--)
    {
              scanf("%s",lst);
              char *p=lst;
              for(;p<lst+strlen(lst);p++)
              {
                  if(strncmp("111110",p,6)==0)
                  {
                      printf("11111");
                      p+=5;
                  }
                  else
                  {
                      printf("%c",*p);
                  }
              }
              printf("\n");
    }
    return 0;    
}
