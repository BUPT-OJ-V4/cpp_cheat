#include<iostream>
#include<cstdio>
#include<cstdlib>
#include<algorithm>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
              char s[1001],str[1001];
              int i,count=0,flag=0;
              scanf("%s",s);
              for(i=0;s[i];)
              {
                   if(s[i]=='1')
                   {
                       if(count<5)
                       {  
                       count++;
                       if(count==5)
                       {
                               if(s[i+1]&&s[i+1]=='0')
                               {
                               s[i+1]=' ';
                              // cout<<i+1<<endl;
                               i+=2;
                               count=0;
                               }
                               
                       }
                       else
                       i++;
                       }   
                   }
                    else
                    {
                        if(count<5)
                        count=0;
                        i++;
                        }        
              }
              for(i=0;s[i];i++)
              if(s[i]!=' ')
              printf("%c",s[i]);
              cout<<endl;
    }
   // system("pause");
    return 0;
}
