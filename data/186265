#include <stdio.h>
#include <string.h>

char str[1010];
int main(){
    int T,t,i,j,len,n;
    
    while(scanf("%d",&T)!=EOF){
        for(t=0;t<T;t++){
            scanf("%s",&str);
            len = strlen(str);
            n = 0;
            for(i=0;i<len;i++){
                if(str[i] == '1')n++;
                if(str[i] == '0' && n < 5)n = 0;
                if(n == 5 && str[i+1] == '0'){
                    for(j=(i+1);j<=len;j++){
                        str[j] = str[j+1];
                    }
                    n = 0;
                }
                
            }
            printf("%s\n",str);
        }
    }
    
    return 0;
}
