#include<iostream>
#include<string>
using namespace std;
int main()
{
    int t;
    while(cin>>t){
        for(int i=0;i<t;i++){
            string s;
            cin>>s;
            for(int j=0;j<s.length();j++){
                if((j+5)<s.length()&&s[j]=='1'&&s[j+1]=='1'&&s[j+2]=='1'&&s[j+3]=='1'&&s[j+4]=='1'&&s[j+5]=='0'){
                    s[j+5]='$';
                     j+=4;
                    }
                   
                }
                for(int k=0;k<s.length();k++){
                    if(s[k]!='$'){
                        cout<<s[k];
                        }
                    }
                    cout<<endl;
            }
        }
        return 0;
    }
