#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main(){
    int i,j,T,num=0;
    char s[1000];
    scanf("%d",&T);
    if(T>=1&&T<=30)
    while(T--){
        scanf("%s",s);
        int len=0;
        len=strlen(s);
        for(i=0;i<len;i++){
            if(s[i]=='1'){
                num++;              
            }else if(s[i]!='1'){
                num=0;
            }
            if(num==5){
                for(j=i+1;j<len;j++){
                    s[j]=s[j+1];                     
                }           
                len=len-1;
                num=0;
            }          
        } 
        for(i=0;i<len;i++){
            printf("%c",s[i]);                   
        }
        printf("\n");
    }
    //system("pause");
    return 0;    
}
