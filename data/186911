#include <iostream>
#include <string>
using namespace std;

int main()
{
    int T,count;
    cin>>T;
    string s,r;

    for(int i=0;i<T;i++)
    {
        r="";
        cin>>s;
        int k=0;
        count=0;
        for(int j = 0;j<s.size();j++)
        {
            r+=s[j];
            k++;
            if(s[j]=='1')
            {
                   
                count++;
                if(count==5)
                {
                    j++;
                    count=0;
                }
            }
            else count=0;
        }
        cout<<r<<endl;
    }
}
