#include <stdio.h>
#include <iostream>//#include <algorithm>
#include <cstring>
using namespace std;

char s[30][1000];
int main()
{
    int a;
    while(scanf("%d",&a)!=EOF)
    {                
      for(int m=0;m<a;m++)
      {cin>>s[m];}
      int count=0;
    for(int i=0;i<a;i++)
    {
       int len=strlen(s[i]);
       int buf[1000]={0};
      for(int j=0;j<len;j++)
      {
              if(s[i][j]=='1')
              {
              count++;
              }             
              if(s[i][j]=='0')
              {count = 0;}              
              if(count == 5)
               {
               buf[j+1]=1;//not output
               count = 0;
               }
       }
       for(int k=0;k<strlen(s[i]);k++)
       {
               if(buf[k]!=1)
               {cout<<s[i][k];}
       }
       cout<<endl;
    }
    }
    //cin>>a;
    return 0;
}