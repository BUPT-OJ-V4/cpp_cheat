#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <vector>
#include <string.h>
#include <string>
#include <algorithm>
#include <queue>
#include <numeric>

using namespace std;

#define MAX_N 55

// int KK[MAX_N];
char str[1024];

int main()
{
    // freopen("in1.txt", "r", stdin);

    int T;
    scanf("%d", &T);

    getchar();
    while(T--)
    {

        gets(str);
        int con = 0;
        // printf("%d\n", strlen(str));
        for(unsigned int i=0; i<strlen(str); i++)
        {
            if(str[i] == '1')
            {
                con++;
                printf("1");
            }
            else
            {
                if(con == 5)
                {
                    con = 0;
                    continue ;
                }
                else
                {
                    con = 0;
                    printf("0");
                }
            }
        }

        printf("\n");
    }

    // printf("%d\n", T);
    return 0;
}
