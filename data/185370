#include<iostream>
#include<string>
using namespace std;

int main(){
int T;
string s;
int len,count;

while(cin>>T){
   for(int i = 0;i < T;i++){
      cin>>s;
      len = s.length();
      for(int j = 0;j < len;j++){
          if(s[j] == '1'){
            count = 0;
            while(s[j] == '1' && count < 5){
                  cout<<s[j];     
                  count++;
                  j++;     
            }//while(s[j] == 1)  
            j--; 
            if(count == 5)
               j++;           
          } 
          else{
              cout<<s[j];        
          } 
      }//for(j = 0;j < len;j++)
      cout<<endl;
   }//for(int i = 0;i < T;i++)
}//while



return 0;    
}
