#include <stdio.h>
#include<string.h>
#include<vector>
using namespace std;
char buf[52],buf1[52];
int main()
{
    int T;
    scanf("%d",&T);
    while(T--){
        memset(buf1,'\0',sizeof(buf1));
     scanf("%s",buf);
     int len=strlen(buf);
     int ans=0,size=0;
     for(int i=0;i<len;i++,size++){
        if(buf[i]=='1')
        {
            ans++;
           if(ans==5){
            buf1[size]=buf[i];
            i++;
            ans=0;
            }
           else buf1[size]=buf[i];
        }
        else{
          buf1[size]=buf[i];
          ans=0;
        }
       }
      for(int i=0;i<size;i++)
        printf("%c",buf1[i]);
    printf("\n");
    }
    return 0;
 }
