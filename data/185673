#include <cstdlib>
#include <iostream>
#include <stdio.h>
#include <string.h>

using namespace std;

char frame[10001];

int main()
{
    int t;
    scanf("%d",&t);
    while(t--){
       scanf("%s",&frame);
       int len=strlen(frame);
       int cnt=0;
       
       for(int i=0;i<len;i++){
          if(frame[i]=='1'){
             cnt++;
             if(cnt==5){
                for(int j=i+2;j<len;j++){
                   frame[j-1]=frame[j];
                }
                len--;
                cnt=0;
             }
          }
          else cnt=0;
       }
       
       for(int i=0;i<len;i++){
          printf("%c",frame[i]);
       }
       printf("\n");
    }
    return 0;
}
