#include <stdio.h>
int main()
{
    int i,j,k=0;
    int T;
    int len;
    char code[1000];
    int flag[1000];
    scanf("%d",&T);
    while(k<T)
    {
        scanf("%s",&code);
        len = strlen(code);
        for(i=0;i<len;i++)
            flag[i]=0;
        for(i=5;i<len;i++)
        {
            if(code[i]=='0'&&code[i-1]=='1'&&code[i-2]=='1'&&code[i-3]=='1'&&code[i-4]=='1'&&code[i-5]=='1')
                flag[i]=1;
        }

        for(i=0;i<len;i++)
        { if(flag[i]!=1)
          printf("%c",code[i]);
        }

        printf("\n");
        k++;
    }

return 0;
}
