#include<cstring>
#include<cstdio>
#include<cmath>
#include<algorithm>
#include<cstdlib>
using namespace std;
char p[1005];
int main()
{
    int i,j,k,n,sum,t,len,flag;
    while(scanf("%d",&t) != EOF){
        while(t --){
            scanf("%s",p);
            len = 0;
            sum = 0;
            n = strlen(p);
            for(i = 0;i < n;i ++){
                if(p[i] == '1'){
                    sum ++;
                }else{
                    sum = 0;
                }
                p[len ++] = p[i];
                if(sum == 5){
                    i ++;
                    sum = 0;
                }
            }
            p[len] = '\0';
            printf("%s\n",p);

        }
    }
    return 0;
}
