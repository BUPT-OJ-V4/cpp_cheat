#include<stdio.h>
#include<string.h>
#include<stdlib.h>
char a[1100];
int main(){
    //freopen("input.txt","r",stdin);
    int T;
    scanf("%d",&T);
    for(int i=0;i<T;i++){
        scanf("%s",a);
        int length=strlen(a);
        int flag=0;
        for(int j=0;j<length;j++){
            if(flag>=5){
                flag=0;
                continue;
            }
            if(a[j]=='1'){
                printf("1");
                flag++;
            }else if(a[j]=='0'){
                flag=0;
                printf("0");
            }
        }
        printf("\n");
    }
    return 0;

}
