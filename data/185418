#include <stdio.h>
#include <stdlib.h>

char arr[1020];

int main()
{
    int T;
    scanf("%d", &T);
    gets(arr);
    while(T--)
    {
        memset(arr, 0, sizeof(arr));
        gets(arr);
        int num = 0;
        int i = 0;
        while(arr[i] != '\0')
        {
            if(arr[i] == '1')
            {
                ++num;
                printf("1");
                if (num == 5)
                {
                    num = 0;
                    ++i;
                }
            }
            else
            {
                num = 0;
                printf("0");
            }
            ++i;
        }
        printf("\n");
    }
    return 0;
}
