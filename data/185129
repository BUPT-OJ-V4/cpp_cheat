#include<iostream>
#include<cstdio>
#include<stdio.h>
#include<queue>
#include<string.h>
#include<vector>
#include<algorithm>
int uuu ;
using namespace std ;
char a[11111] ;
char ans[11111] ;
int main()
{
    int T ;
    cin>>T ;
    while(T--)
    {
        cin>>a ;
        int i ;
        int len = strlen(a) ;
        int count = 0;
        int p = 0 ;
        for(i=0;i<len;i++)
        {
            if(a[i]=='1')  count ++ ;
            else count = 0 ;
            
            if(count == 5)
            {
                  ans[p] = a[i] ;
                  p++ ; 
                  i++ ;
                count = 0 ;continue ;
            }
            
            ans[p] = a[i] ;
            p++ ;
        }
        ans[p] = '\0' ;
        cout<<ans<<endl;
    }
  //  cin>>uuu ;
    return 0 ;
}
