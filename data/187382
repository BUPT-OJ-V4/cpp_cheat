#include<iostream>
using namespace std;
int main()
{
   
    long long i,j,n,m,count;
    int t;
    while(cin>>t)
    {
      while(t--)
      {
                count=0;
                cin>>n>>m;
                if(n>=m)
                {
                   for(i=2;i<=m;i++)
                   {
                   if(m%i==0)
                   count++;
                    }
                  cout<<count+1<<endl;
                }
                else if(m>n*n)
                cout<<count<<endl;
                else
                {
                  for(i=2;i<=n;i++)
                  {
                    if((m%i==0)&&(i*n>=m))
                    count++;
                     
                  }
                  cout<<count<<endl;
                }
                
      }           
    }
    
    
    
    return 0;
}
