#include <stdio.h>

int main()
{
    int t, p;
    scanf("%d", &t);
    for(p=1; p<=t; p++)
    {
        long n, m;
        long i;
        int sum = 0;
        scanf("%ld %ld", &n, &m);
        for(i=1; i<=n; i++)
        {
            long k;
            if(m % i ==0)
            {
                k = m/i;
                if(k <= n) sum++;
            }
        }
        printf("%d\n", sum);
    }
    return 0;

}
