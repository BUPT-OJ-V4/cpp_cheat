#include<stdio.h>
#include<stdlib.h>
int main(){
    int T,t,n,i,j,k,a[100],ans;
    while(scanf("%d",&T)!=EOF){;
    for(t=0;t<T;t++){
          ans=0;
          scanf("%d",&n);
          for(i=0;i<n;i++)
            scanf("%d",&a[i]);
          for(i=0;i<n;i++)
          for(j=0;j<n;j++)
          for(k=0;k<n;k++)
            if(a[i]+a[j]==a[k])
               ans++;
          printf("%d\n",ans);          
}
}
    //system ("pause");
    return 0;
}