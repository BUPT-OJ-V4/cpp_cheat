#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()
{
    int t,i,j,len,count;
    char a[1001];
    while(scanf("%d",&t)!=EOF)
    {
                              while(t--)
                              {
                                   count=0;
                                   scanf("%s",a);
                                   len=strlen(a);
                                   for(i=0;i<len;i++)
                                   {
                                                     if(a[i]=='1'&&count!=5)
                                                     {
                                                                  count++;
                                                                  printf("%c",a[i]);
                                                     }
                                                     if(a[i]=='0'&&count!=5)
                                                     {
                                                                  printf("%c",a[i]);
                                                                  count=0;
                                                     }
                                                     else
                                                     if(a[i]=='0'&&count==5)
                                                     {
                                                                  count=0;
                                                     }

                                   }
                                   printf("\n");
                              }
    }
    //stem("pause");
    return 0;
}
   
                                                                  
                                                                  
                                   
                                   
