#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main(){
    int t;
    int i,j;
    char a[1001];
    char b[1001];
    int l,k;
    int count;
    int m;
    while(scanf("%d",&t)!=EOF){
       for(i=0;i<t;i++){
          for(m=0;m<1001;m++)              
            a[m]= '\0';
          for(m=0;m<1001;m++)              
            b[m]= '\0';
          scanf("%s",&a);  
          
          l=0;
          k=0;
          count =0;
          while(a[l]!='\0'){
             l++;                  
          }  
          
          for(j=0;j<l;j++){
            if(a[j]=='1'){
               
               if(count==5)
                 count = 0;
               b[k]=a[j];
               count ++;
            }
            if(a[j]=='0'){
               if(count == 5){
                 count = 0;
                 k--;
               }
               else{
                b[k]=a[j];  
                count = 0;
                }            
            }
            k++;
          } 
          b[k+1]='\0';  
          printf("%s\n",b); 
       }     
    }    
        
   
    return 0;    
}
