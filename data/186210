#include <stdlib.h>
#include <stdio.h>
#include <string.h>

int main(){
	int n;
	while(scanf("%d",&n)!=EOF){
		for(int i=0;i<n;i++){
			char str[1000];
			char ans[1000];
			scanf("%s",str);
			int mark=0,j=0,m;
			for(m=0;m<strlen(str);m++){
				if(str[m]=='1'&&mark<5){
					ans[j]=str[m];
					j++;
					mark++;
				}
				else if((str[m]=='0')&&(mark<5)){
					ans[j]=str[m];
					j++;
					mark=0;
				}
				else if(mark==5){
					mark=0;
				} 
			}
			for(int k=0;k<j;k++){
				printf("%c",ans[k]);
			}
			printf("\n");
		}
	}
	return 0;
}