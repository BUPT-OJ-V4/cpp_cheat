#include <stdio.h>

int main()
{
     int a;
     scanf("%d",&a);
     while(a--){

              int m,n,i,j,count=0;
              scanf("%d",&n);
              scanf("%d",&m);
              
              //int **a;
              //a=(int**)malloc(sizeof(int*)*n);
              //for(i=0;i<n;i++)
               //               a[i]=(int*)malloc(sizeof(int)*n);
              for(i=n;i>0;i--)
              if(m%i==0&&m/i<=n)
              count++;
              printf("%d\n",count);
                            
     }
     
     return 0;
}
