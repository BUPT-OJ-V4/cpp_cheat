#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main(){
    int t,i,j,k=0;
    char b[30][1000]={' '};
    while(scanf("%d",&t)!=EOF){
        for(i=0;i<t;i++){
            scanf("%s",b[i]);
            for(j=0;j<strlen(b[i]);j++){
                if((int)b[i][j]-48==1) ++k;
                if((int)b[i][j]-48==0) k=0;
                if(k==5){
                    for(k=j+1;k<strlen(b[i]);k++)
                        b[i][k]=b[i][k+1];
					k=0;
                }//if
            }//for,j
            k=0;
        }//for,i
		k=0;
        for(i=0;i<t;i++){
            printf("%s\n",b[i]);
        }
        for(i=0;i<t;i++){
            for(j=0;j<strlen(b[i]);j++){
                b[i][j]=' ';
            }
        }
    }//while
 return 0; 
}
